    <script type="text/javascript" src="{{ jslink('js/jquery.fancybox.js') }}"></script>
    <script type="text/javascript" src="{{ jslink('js/jquery.elevatezoom.js') }}"></script>
    <script type="text/javascript" src="{{ jslink('js/jquery.jcarousellite.js') }}"></script>
    <script>
        $( document ).ready(function() {
            function detail_gallery(){
                if($('.detail-gallery').length>0){
                    $('.detail-gallery').each(function(){

                        $(this).find(".carousel").jCarouselLite({
                            btnNext: $(this).find(".gallery-control .next"),
                            btnPrev: $(this).find(".gallery-control .prev"),
                            speed: 800,
                            visible: 4,
                            mouseWheel: true
                        });
                        //Elevate Zoom
                        if ($(window).width() > 767) {
                        if ( $('html').attr('lang') == 'ar' ) {
                        $(this).find('.mid img').elevateZoom({
                            cursor: "pointer",
                            zoomWindowFadeIn: 500,
                            zoomWindowPosition: 9,
                            zoomType : "window",
                            gallery:'zoomGallery',
                            zoomWindowFadeOut: 750
                        });
                        $("#zoom_01").bind("click", function(e) {
                            var ez = $('#zoom_01').data('elevateZoom');
                            $.fancybox(ez.getGalleryList());
                            return false;
                        });
                        } else {
                        $(this).find('.mid img').elevateZoom({
                            cursor: "pointer",
                            zoomWindowFadeIn: 500,
                            zoomType : "window",
                            gallery:'zoomGallery',
                            zoomWindowFadeOut: 750
                        });
                        $("#zoom_01").bind("click", function(e) {
                            var ez = $('#zoom_01').data('elevateZoom');
                            $.fancybox(ez.getGalleryList());
                            return false;
                        });
                        }}
                        else {
                            if ( $('html').attr('lang') == 'ar' ) {
                                $(this).find('.mid img').elevateZoom({
                                    cursor: "pointer",
                                    zoomWindowFadeIn: 500,
                                    zoomWindowPosition: 10,
                                    zoomType : "inner",
                                    gallery:'zoomGallery',
                                    galleryActiveClass: 'active',
                                    zoomWindowFadeOut: 750
                                });

                                } else {
                                $(this).find('.mid img').elevateZoom({
                                    cursor: "pointer",
                                    zoomWindowFadeIn: 500,
                                    zoomType : "inner",
                                    zoomWindowFadeOut: 750
                                });
                            }

                        }
                        $(this).find(".carousel a").on('click',function(event) {
                            event.preventDefault();
                            $(this).parents('.detail-gallery').find(".carousel a").removeClass('active');
                            $(this).addClass('active');
                            $(this).parents('.detail-gallery').find(".mid img").attr("src", $(this).find('img').attr("src"));
                            var z_url = $(this).parents('.detail-gallery').find('.mid img').attr('src');
                            $('.zoomWindow').css('background-image','url("'+z_url+'")');
                        });
                    });
                }
            }
            detail_gallery();
        });
    </script>

