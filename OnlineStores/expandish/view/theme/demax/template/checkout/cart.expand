<div class="container">
    {% if attention %}
    <br>
    <div class="alert alert-warning alert-dismissible" role="alert">
        <button type="button" class="close" data-dismiss="alert" aria-label="Close">
            <span aria-hidden="true">&times;</span>
        </button> {{ attention }}
    </div>
    {% endif %} {% if success %}
    <br>
    <div class="alert alert-success alert-dismissible" role="alert">
        <button type="button" class="close" data-dismiss="alert" aria-label="Close">
            <span aria-hidden="true">&times;</span>
        </button> {{ success }}
    </div>
    {% endif %} {% if error_warning or error_stock_forecasting %}
    <br>
    <div class="alert alert-warning alert-dismissible" role="alert">
        <button type="button" class="close" data-dismiss="alert" aria-label="Close">
            <span aria-hidden="true">&times;</span>
        </button>  {{ error_warning }} <br> {{ error_stock_forecasting }}
    </div>
    {% endif %}
</div>

<!-- Breadcrumbs -->
<div class="breadcrumbs">
    <div class="container">
        <div class="row">
            <div class="col-xs-12">
                <ul>
                    {% for breadcrumb in breadcrumbs %} {% if loop.last %}
                    <li>
                        <strong>{{ breadcrumb['text'] }}</strong>
                    </li>
                    {% else %}
                    <li class="home">
                        <a title="Go to Home Page" href="{{ breadcrumb['href'] }}">{{ breadcrumb['text'] }}</a>
                        <span>&raquo;</span>
                    </li>
                    {% endif %} {% endfor %}
                </ul>
            </div>
        </div>
    </div>
</div>
<!-- Breadcrumbs End -->

<!-- Main Container -->
<form class="form-cart" id="cart-form" action="{{ link('checkout/cart') }}" method="post" enctype="multipart/form-data">

<section class="main-container col1-layout">
