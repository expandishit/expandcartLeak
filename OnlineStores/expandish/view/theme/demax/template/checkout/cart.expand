                                        </td>
                                        <td class="cart_description hidden-sm ">
                                            <p class="product-name">
                                                <a href="{{ product['href'] }}">{{ product['name'] }}{% if not product['stock'] %} <strong><span class="stock text-danger text-capitalize ">{{ lang('error_quantity') }}</span></strong>{% endif %}</a><br>
                                                <small>{{ product['reward'] }}</small>
                                            </p>
                                            {% set unique_option_names = [] %}
                                            {% for option in product['option'] %}
                                                {% if option['name'] not in unique_option_names %}
                                                    {% set unique_option_names = unique_option_names|merge([option['name']]) %}
