                                        <td class="price ">
                                            <span>{{ product['price'] }}</span>
                                        </td>

                                        <td class="qty ">
                                            <input type="text" name="quantity[{{ product['key'] }}]" title="{{ lang('column_quantity') }}" class="form-control input-sm input-qty"
