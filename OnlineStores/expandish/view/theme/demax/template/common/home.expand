						<div class="mini-cart">
					  		<div data-hover="dropdown" class="basket dropdown-toggle">
						  		<a href="{{ link('checkout/cart') }}">
									<div class="cart-icon block-minicart">
										<i class="icon-basket-loaded icons"></i>
