				<div class="mega-menu-title">
				  <h3><span>{{ header_data.FeaturedCategories.fields.FeaturedCategories_Title.value }}</span></h3>
				</div>
					  {% else %}
						  {%  set headerColsize = 12  %}
				  {% endif %}

				<!-- Shop by category -->
