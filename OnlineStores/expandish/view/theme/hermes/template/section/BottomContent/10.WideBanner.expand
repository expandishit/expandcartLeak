
{% for collection in collections %}
<div class="container question-faq"  id="section-{{ section_id }}" style="background-color: {{ collection.Banner_BGColor.field_value }}" id="section-{{ section_id }}">
        <span style="color: {{ collection.Banner_Color.field_value }}">{{ collection.Banner_Title.field_value }}</span> <a href="{{ collection.Banner_Link.field_value }}"><i class="fa fa-arrow-circle-right" aria-hidden="true"></i></a>
</div>
{% endfor %}