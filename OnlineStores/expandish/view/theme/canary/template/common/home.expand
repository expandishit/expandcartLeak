								<div class="search-form form-search" id="search">
									<form>
										<input type="text" name="search" class="form-control" placeholder="{{ lang('text_search') }}" value="{{ search }}">
									</form>
								</div>

								<div class="register-box">
									<a class="btn btn-search button-search link-signin" type="button">{{ lang('text_searchbtn') }}</a>
								</div>
							</div>
						</div>
