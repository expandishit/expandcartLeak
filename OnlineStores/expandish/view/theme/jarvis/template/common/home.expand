                                                        <li class="block-container col-sm-3">
                                                            <ul class="block-megamenu-link">
                                                                {% for category in cateVal|slice(halfCat) %}
                                                                <li class="link_container">
                                                                    <a href="{{ category['href'] }}">
                                                                        {{ category['name'] }}
                                                                    </a>
                                                                </li>
                                                                {% endfor %}
                                                            </ul>
                                                        </li>
                                                    {%else%}
                                                        <li class="block-container col-sm-3">
                                                            <ul class="block-megamenu-link">
                                                                {% for category in cateVal%}
                                                                <li class="link_container">
                                                                    <a href="{{ category['href'] }}">
                                                                        {{ category['name'] }}
                                                                    </a>
                                                                </li>
                                                                {% endfor %}
                                                            </ul>
                                                        </li>
                                                    {% endif%}
                                                </ul>
                                            </li>
                                            {% endif %}
                                            <!--- New Dropdown --->
                                            <!--- New Link Static --->
                                            {% if header_data.LinkPageStatic.section_state == 'enabled' %}
                                            {% for collection in header_data.LinkPageStatic.collections %}
                                            <li class="dropdown">
                                                <a href="{{collection.PPage_Link.value}}" class="dropdown-toggle">
                                                    {{collection.PPage_Title.value}}
                                                </a>
                                            </li>
                                            {%endfor%}
                                            {% endif %}
                                            <!--- New Link Static --->
		                                </ul>
		                                <ul class="nav navbar-nav navbar-right">
                                         {% if customer.isCustomerAllowedToAdd_cart %}
								            <li>
								            	<div class="block-wrap-cart">
													<div class="iner-block-cart block-minicart">
														<a href="{{ link('checkout/cart') }}">{{ lang('text_shopping_cart') }}<span class="total counter-number"> ({{ cart.countProducts() }})</span>
														</a>
													</div>
													<div class="block-mini-cart block-minicart">
														<div class="mini-cart-content cart_items">
                                                            {{ cart_content }}
													</div>
												</div>
								            </li>                         
                                         {% endif %}
							            </ul>
		                            </div><!--/.nav-collapse -->
		                        </div>
		                    </nav>
						</div>
					</div>
				</div>
                <!-- ./main menu-->
                {##% endif %##}
			</div>
		</div>
	</header>
    <!-- ./header -->

