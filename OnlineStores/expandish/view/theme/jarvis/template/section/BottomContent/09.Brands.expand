<div class="list-brand" style="margin: 20px 0;">
    <div class="container">
        <div class="list-brand-slider kt-owl-carousel kt-owl-option5" data-margin="0" data-loop="true" data-nav="true" data-responsive='{"0":{"items":2},"600":{"items":3},"768":{"items":4},"1000":{"items":5},"1200":{"items":6}}'>
            {% for collection in collections %}
            <div class="item-brand"><a href="{{ collection.Brand_Link.field_value }}"><img src="{{ collection.Brand_Image.field_value }}" alt="" /></a></div>
            {% endfor %}
        </div>
    </div>
</div>
<!-- End List Brand -->