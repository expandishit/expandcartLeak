<div class="brand-slider5 line-white" id="section-{{ section_id }}">
  <div class="wrap-item" data-itemscustom="[[0,2],[480,3],[768,4],[990,5],[1200,6]]" data-pagination="false" data-autoplay="true">
    {% for collection in collections %}
    <div class="brand-item5">
      <a href="{{ collection.imagelink.field_value }}" class="wobble-horizontal"><img src="{{ collection.slideimage.field_value }}" alt="" /></a>
    </div>
    {% endfor %}
  </div>
</div>