<!DOCTYPE HTML>
<html lang="{{ lang('code') }}" dir="{{ lang('direction') == 'rtl' ? 'rtl' : '' }}">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1" />

    <!-- Basic Tags -->
    <base href="{{ base_url }}" />

    {% if document_description != '' %}
        <meta name="description" content="{{ document_description }}" />
    {% endif %}

    {% if document_keywords != '' %}
        <meta name="keywords" content="{{ document_keywords }}" />
    {% endif %}

    {% if store_icon != '' %}
        <link href="{{ store_icon }}" rel="icon" />
    {# <meta property="og:image" content="{{ store_icon }}"/> #}

    {% endif %}

    <title>{{ document_title }}</title>

    {% if route == "product/product" %}
      {% include 'default/template/structured_data/facebook_og.expand' %}
    {% else %}
      {% include 'default/template/structured_data/store.expand' %}
    {% endif %}

    <!-- Documets Links -->
    {% for link in document_links %}
    <link href="{{ link.href }}" rel="{{ link.rel }}" />
    {% endfor %}

    <!-- CSS Files -->
    {% for style in document_styles %}
        <link rel="{{ style.rel }}" type="text/css" href="{{ style.href }}" media="{{ style.media }}" />
    {% endfor %}

    <link rel="stylesheet" type="text/css"
          href="index.php?route=common/style{{ isdraft == 1 ? '&isdraft=1' : '' }}{{ config('tmp_prev') ? '&__p=' ~ config('tmp_prev') : '' }}">

    <!-- jQuery 1.10.1-->
    <script src="{{ jslink('js/libs/jquery-1.12.0.min.js') }}"></script>
    <script type="text/javascript" src="{{ jslink('js/libs/jquery-ui.min.js') }}"></script>
    <link rel="stylesheet" type="text/css" href="{{ csslink('css/jquery-ui-1.8.16.custom.css') }}" />
    <link rel="stylesheet" type="text/css" href="{{ csslink('js/jquery/ui/themes/ui-lightness/jquery-ui-1.8.16.custom.css') }}" />
    

    {% for script in document_scripts %}
        <script type="text/javascript" src="{{ script }}"></script>
    {% endfor %}

    <!-- Inline Script:Start -->
    {% for script in inline_scripts %}
        {% if script['type'] == 'callable' %}
            {{ script['script']|base64_decode }}
        {% endif %}
    {% endfor %}
    <!-- Inline Script:End -->

    <!-- Google Analytics -->
    {{ google_analytics }}

  	<!-- Gameball -->
  {% if gameballApiKey %}

  		<script>
  		{% if logged == false  %}
              window.GbLoadInit = function(){
                  GbSdk.init({
                      lang: '{{ lang('code') }}',
                      APIKey: '{{ gameballApiKey }}'
                  });
              };
            {% else %}
            window.GbLoadInit = function(){
                    GbSdk.init({
                        playerUniqueId: '{{ customer.getId() }}',
                        playerAttributes: {
                            displayName: '{{ customer.getFirstName() }}',
                            email: '{{ customer.getEmail() }}'
                        },
                        lang: '{{ lang('code') }}',
                        APIKey: '{{ gameballApiKey }}'
                    });
                };
            {% endif %}
          </script>

          <script defer src="https://assets.gameball.co/widget/js/gameball-init.min.js"></script>
      {% endif %}

</head>


<body class="boxed" style="background:#f5f5f5">

        {# include top banner app html ("topBannerFixed" parameter used to show fixed layout) #}
        {% include "default/template/module/top_banner/index.expand" with {'topBannerFixed': false} %}

        <nav class="{{ lang('direction') == 'rtl' ? 'push-sidebar push-sidebar-right' : 'push-sidebar' }} hidden" id="sidebar-left">
        <ul class="nav  nav-stacked sidebar ">
    
            {% if header_data.mainMenuLink.section_state == 'enabled' %}
            {% for collection in header_data.mainMenuLink.collections %}
            <li class="">
                <a href="{{ collection.itemlink.value }}">{{ collection.itemname.value }}</a>
            </li>
            {% endfor %}
            {% endif %}
            
            {% if header_data.megaMenuCategories.section_state == 'enabled' %}
            {% for collection in header_data.megaMenuCategories.collections %}
                {% set menu_item_category = getCategoryAndChilds(collection['categories']['value'], 170, 100) %}
                {% for menu_category in menu_item_category %}
                    {% set cat_sub = menu_category["sub_categories"]|length %}
                    {% if  cat_sub == 0 %}
                        <li class="">
                            <a href="{{ menu_category.href }}">{{menu_category.name }}</a>
                        </li>
                    {% else %} 
                        <li role="presentation" class="dropdown">
                            <a class="dropdown-toggle" data-toggle="dropdown" href="{{ menu_category.href }}" role="button" aria-haspopup="true" aria-expanded="false">
                                {{menu_category.name }} <i class="fa fa-angle-down"></i>
                            </a>
                            <ul class="dropdown-menu">
                                
                                {% for sub_category in menu_category["sub_categories"]|slice(0,9) %}
                                <li>
                                    <a href="{{ sub_category.href }}">{{ sub_category.name }}</a>
                                </li>
                                {% endfor %}
                                
                            </ul>
                        </li>
                    {% endif %}
                {% endfor %}
            {% endfor %}
            {% endif %} 

            {% if header_data.mainMenuDropdown1.fields.link.value != '' and header_data.mainMenuDropdown1.section_state == 'enabled' %}
              <li role="presentation" class="dropdown">
                    <a class="dropdown-toggle" data-toggle="dropdown" href="{{ header_data.mainMenuDropdown1.fields.link.value}}" role="button" aria-haspopup="true" aria-expanded="false">
                            {{ header_data.mainMenuDropdown1.fields.linkname.value}} <i class="fa fa-angle-down"></i>
                    </a>
                    <ul class="dropdown-menu">
                        {% for collection in header_data.mainMenuDropdown1.collections %}
                        <li>
                            <a href="{{collection.link.value}}">{{collection.linkname.value}}</a>
                        </li>
                        {% endfor %}
                    </ul>
                </li>
            {% endif %}

            {% if header_data.mainMenuCategories.section_state == 'enabled' %}
            {% for collection in header_data.mainMenuCategories.collections %}
                {% set menu_item_category = getCategoryAndChilds(collection['categories']['value'], 170, 100) %}
                {% for menu_category in menu_item_category %}
                    {% set cat_sub = menu_category["sub_categories"]|length %}
                    {% if  cat_sub == 0 %}
                        <li class="">
                            <a href="{{ collection.itemlink.value }}">{{ collection.itemname.value }}</a>
                        </li>
                    {% else %} 
                        <li role="presentation" class="dropdown">
                            <a class="dropdown-toggle" data-toggle="dropdown" href="{{ collection.itemlink.value }}" role="button" aria-haspopup="true" aria-expanded="false">
                                {{ collection.itemname.value }} <i class="fa fa-angle-down"></i>
                            </a>
                            <ul class="dropdown-menu">
                                
                                {% for sub_category in menu_category["sub_categories"]|slice(0,9) %}
                                <li>
                                    <a href="{{ sub_category.href }}">{{ sub_category.name }}</a>
                                </li>
                                {% endfor %}
                                
                            </ul>
                        </li>
                    {% endif %}
                {% endfor %}
            {% endfor %}
            {% endif %} 
    
    
            {% if header_data.mainMenuLinkEnd.section_state == 'enabled' %}
            {% for collection in header_data.mainMenuLinkEnd.collections %}
            <li class="">
                <a href="{{ collection.itemlink.value }}">{{ collection.itemname.value }}</a>
            </li>
            {% endfor %}
            {% endif %}
    
    
        </ul>
    </nav>

    <div class="wrap canvas" id="canvas">
        <header id="header">
            <div class="header-main">
                <div class="container">
                    <div class="row">
                        <div class="col-md-5 col-sm-5 col-xs-12">
                            <ul class="list-inline-block top-menu">
                                {% if logged == true  %}
                                <li><a class="title14 black" href="{{ link('account/account') }}"> {{ lang('Account_Account')  }}</a></li>
                                <li><a class="title14 black" href="{{ link('account/logout', '', 'SSL') }}">{{ lang('AccountLogout')|format(link('account/account', '', 'SSL'), customer.getFirstName(), link('account/logout', '', 'SSL')) }}</a></li>
                                {% endif %}
                                {% if logged == false  %}
                                    {# <li><a class="title14 black" href="{{link('account/login', '', 'SSL')}}">{{ lang('SignIn_Title')|format(link('account/login', '', 'SSL'), link('account/register', '', 'SSL')) }}</a></li>
                                    <li><a class="title14 black" href="{{ link('account/register', '', 'SSL') }}">{{ lang('SignUp_Register')|format(link('account/login', '', 'SSL'), link('account/register', '', 'SSL')) }}</a></li> #}
                                    <li>{% include 'default/template/common/login_link.expand' with {custom_class: 'title14 black'} %}</li>
                                    {% if multiseller == true %}<li>{% include 'default/template/common/login_seller_link.expand' with {custom_class: 'title14 black'} %}</li>{% endif %}
                                {% endif %}
                                <li><a href="{{ link('checkout/checkout') }}" class="title14 black">{{ lang('button_checkout') }}</a></li>
                                
                                {% if header_data.topHeaderLinKs.section_state == 'enabled' %}
                                    {% for collection in header_data.topHeaderLinKs.collections %}
                                        <li><a class="title14 black" href="{{ collection.itemlink.value }}">{{ collection.itemname.value }}</a></li>
                                    {% endfor %}
								{% endif %}
                            </ul>
                            {% if languages|length > 1 or currencies|length > 1 %}
                            <ul class="list-inline-block top-currency-language">
                                {% if languages|length > 1 %}
                                <li>
                                    <div class="language-box">
                                        <a href="#" class="language-current title14 black"><span>{{ current_language.name }}</span><i class="fa fa-angle-down" aria-hidden="true"></i> </a>
                                        <form method="post" enctype="multipart/form-data">
                                            <ul class="language-list list-none">
                                                {% for language in languages %}
                                                <li>
                                                    <a href="#" title="{{ language.name }}" onclick="$('input[name=\'language_code\']').attr('value', '{{ language.code }}'); $(this).closest('form').submit(); return false;">
                                                        <img src="admin/view/image/flags/{{ language.image }}" alt="{{ language.name }}" /><span>{{ language.name }}</span>
                                                    </a>
                                                </li>
                                                {% endfor %}
                                                <input type="hidden" name="language_code" value="" />
                                                <input type="hidden" name="redirect" value="{{ redirect }}" />
                                            </ul>
                                        </form>
                                    </div>
                                </li>
                                {% endif %}
                                {% if currencies|length > 1 %}
                                <li>
                                    <div class="currency-box">
                                        <a href="#" class="currency-current title14 black"><span>{{ current_currency.title }}</span><i class="fa fa-angle-down" aria-hidden="true"></i></a>
                                        <form action="{{ link('common/home') }}" method="post" enctype="multipart/form-data">
                                            <ul class="currency-list list-none">
                                                {% for currency in currencies %}
                                                <li>
                                                    <a href="#" onclick="$('input[name=\'currency_code\']').attr('value', '{{ currency.code }}'); $(this).closest('form').submit(); return false;">
                                                        <span class="currency-index">{{ currency.symbol }}</span>
                                                    </a>
                                                </li>
                                                {% endfor %}
                                            </ul>
                                            <input type="hidden" name="currency_code" value="" />
                                            <input type="hidden" name="redirect" value="{{ redirect }}" />
                                        </form>
                                    </div>
                                </li>
                                {% endif %}
                            </ul>
                            {% endif %}
                        </div>
                        <div class="col-md-2 col-sm-2 col-xs-7">
                            {% if header_data.logoblock.section_state == 'enabled' %}
                            <div class="logo logo1">
                                <h1 class="hidden">{{ store_name }}</h1>
                                <a href="{{ link('common/home') }}"><img src="{{ image(header_data.logoblock.fields.logo.value) }}" alt="{{ store_name }}"  style="height: {{ header_data.logoblock.fields.logoheight.value }}px;"/></a>
                            </div>
                            {% endif %}
                        </div>
                        {% if customer.isCustomerAllowedToAdd_cart %}
                        <div class="col-md-5 col-sm-5 col-xs-5">
                            <div class="mini-cart-box pull-right">
                                <a class="mini-cart-link title12" href="{{ link('checkout/cart') }}">
                                    <span class="mini-cart-icon inline-block"><img src="./expandish/view/theme/{{Template_Name}}/images/icons/icon-cart.png" alt="" /><sup class="bg-color white round counterLabel">{{ cart.countProducts() }}</sup></span>
                                    <span class="mini-cart-number  black inline-block text-right ">{{ lang('text_shop_cart') }}<br><span class="counterLabel">{{ cart.countProducts() }}</span> {{lang('items')}} <!-- - <span class="color">$0.00</span> --></span>
                                </a>
                                <div class="mini-cart-content text-left cartDropList">
                                    {{cart_content}}
                                </div>
                            </div>
                        </div>
                        {% endif %}

                    </div>
                </div>
            </div>
            <!-- End Header Main -->
            <div class="header-nav">
                <div class="container">
                    <div class="nav-search">
                        <div class="row">
                            <div class="col-md-12 col-sm-12 col-xs-12">
                                <nav class="main-nav main-nav1">
                                    <ul class="list-none">
                                        {% if header_data.mainMenuLink.section_state == 'enabled' %}
                                        {% for collection in header_data.mainMenuLink.collections %}
                                        <li>
                                            <a href="{{ collection.itemlink.value }}">{{ collection.itemname.value }}</a>
                                        </li>
                                        {% endfor %}
                                        {% endif %}

                                        {% if header_data.megaMenuCategories.section_state == 'enabled' %}
                                        {% for collection in header_data.megaMenuCategories.collections %}
                                        {% set sideADV = collection.displaySidead.value %}
                                        {% set menu_item_category = getCategoryAndChilds(collection['categories']['value'], 170, 100) %}
                                        {% set categories_count = menu_item_category|length %}
                                        {% set cat_width = 3 %}
                                        {% if sideADV %}
                                            {% if categories_count == 1 %}
                                                {% set cat_width = 6 %}
                                            {%endif%}
                                            {% if categories_count == 2 %}
                                                {% set cat_width = 4 %}
                                            {%endif%}
                                            {% if categories_count == 3 %}
                                                {% set cat_width = 3 %}
                                            {%endif%}
                                            {% if categories_count > 4 %}
                                                {% set cat_width = 2 %}
                                            {%endif%}
                                        {% else %}
                                            {% if categories_count == 1 %}
                                                {% set cat_width = 12 %}
                                            {%endif%}
                                            {% if categories_count == 2 %}
                                                {% set cat_width = 6 %}
                                            {%endif%}
                                            {% if categories_count == 3 %}
                                                {% set cat_width = 4 %}
                                            {%endif%}
                                            {% if categories_count == 4 %}
                                                {% set cat_width = 3 %}
                                            {%endif%}
                                            {% if categories_count > 5 %}
                                                {% set cat_width = 2 %}
                                            {%endif%}
                                        {% endif %}
                                        <li class="menu-item-has-children has-mega-menu">
                                            <a href="{{ collection.link.value }}">{{ collection.linkname.value }}</a>
                                            <div class="mega-menu">
                                                <div class="row">
                                                    {% for menu_category in menu_item_category %}
                                                    <div class="col-md-3 col-sm-4 col-xs-12">
                                                        <div class="mega-product">
                                                            <h2 class="title18 title-box5"><a href="{{ menu_category.href }}">{{ menu_category.name }}</a></h2>
                                                            <div class="list-mega-product">
                                                                {% for sub_category in menu_category["sub_categories"]|slice(0,8) %}
                                                                <h3 class="product-title title14"><a href="{{ sub_category.href }}">{{ sub_category.name }}</a></h3>
                                                                {% endfor %}
                                                            </div>
                                                        </div>
                                                    </div>
                                                    {% endfor %}
                                                    {% if sideADV %}
                                                    <div class="col-sm-{{cat_width}}">
                                                        <div class="banner-adv fade-out-in"><a class="adv-thumb-link" href="{{collection.sidedadlink.value}}"><img src="{{ image(collection.sideAdimg.value) }}" /></a></div>
                                                    </div>
                                                    {% endif %}
                                                </div>
                                            </div>
                                        </li>
                                        {% endfor %}
                                        {% endif %}

                                        {% if header_data.mainMenuDropdown1.fields.link.value != '' and header_data.mainMenuDropdown1.section_state == 'enabled' %}
                                        <li class="menu-item-has-children">
                                            <a href="{{ header_data.mainMenuDropdown1.fields.link.value}}"  class="dropdown-toggle">
                                                {{ header_data.mainMenuDropdown1.fields.linkname.value}}
                                            </a>
                                            <ul class="sub-menu">
                                            {% for collection in header_data.mainMenuDropdown1.collections %}
                                            <li><a href="{{collection.link.value}}">{{collection.linkname.value}}</a></li>
                                            {% endfor %}
                                            </ul>
                                        </li>
                                        {% endif %}

                                        {% if header_data.mainMenuCategories.section_state == 'enabled' %}
                                        {% for collection in header_data.mainMenuCategories.collections %}
                                        {% set menu_item_category = getCategoryAndChilds(collection['categories']['value'], 170, 100) %}
                                        <li class="menu-item-has-children">
                                            <a href="{{ collection.itemlink.value }}">{{ collection.itemname.value }}</a>
                                            <ul class="sub-menu">
                                                {% for menu_category in menu_item_category %}
                                                {% set subLength =  menu_category["sub_categories"]|length %}
                                                {% for sub_category in menu_category["sub_categories"]|slice(0,8) %}
                                                <li class="">
                                                    <a href="{{ sub_category.href }}">{{ sub_category.name }}</a>
                                                </li>
                                                {% endfor %}
                                                {% if subLength > 8	 %}
                                                <li><a href="{{ menu_category.href }}"><small>{{ lang('button_view_all') }}</small> {{ lang('direction') == 'rtl' ? '<i class="fa-chevron-circle-left fa muted"></i>' : '<i class="fa-chevron-circle-right fa muted"></i>' }}  </a></li>
                                                {% endif %}
                                                {% endfor %}
                                            </ul>
                                        </li>
                                        {% endfor %}
                                        {% endif %}
                                        
                                        {% if header_data.mainMenuLinkEnd.section_state == 'enabled' %}
                                        {% for collection in header_data.mainMenuLinkEnd.collections %}
                                        <li>
                                            <a href="{{ collection.itemlink.value }}">{{ collection.itemname.value }}</a>
                                        </li>
                                        {% endfor %}
                                        {% endif %}
                                    </ul>
                                    <a data-toggle="push" data-target="#sidebar-left" data-distance="{{ lang('direction') == 'rtl' ? '-250' : '250' }}" title="Open navigation" class="toggle-mobile-menu"><span></span></a>
                                </nav>
                                <!-- End Main nav -->
                                <div class="smart-search smart-search1" id="search">
                                    <span class="title14 search-label">{{ lang('text_searchbtn') }}</span>
                                    <form class="smart-search-form search-form">
                                        <input name="search"  placeholder="{{ lang('text_search') }}" value="{{ search }}" type="text">
                                        <input value="" class="btn-search button-search" type="button">
                                    </form>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </header>
        <!-- End Header -->
        <section id="content">
            <div class="container">
                {% if is_home_page %}
                <div id="notification" class=""></div>
                <div>
                    {{ regions_data.maincontent.content }}
                </div>
                {% elseif not is_home_page and route starts with 'product/' %}
                    {{ page_content }}
                    {{ regions_data.sidebar.content }}
                    {{ regions_data.bottomcontent.content }}
                {% else %}
                    <div>
                    <!-- add condition for the wrapper div to handle the filters -->
                    {{ page_content }}
                    {{ regions_data.bottomcontent.content }}
                    </div>
                {% endif %}
            </div>
        </section>

        <section class="container">
            {% if footer_data.featuresboxes.section_state == 'enabled' %}
            <div class="list-service" id="section-{{ footer_data.featuresboxes.section_id }}">
                <div class="row">
                    {% for collection in footer_data.featuresboxes.collections %}
                    {% set box_width = '12' %}
                    {% if collection.boxwidth.value == '1/4' %}
                        {% set box_width = '3' %}
                    {% elseif collection.boxwidth.value == '1/2' %}
                        {% set box_width = '6' %}
                    {% elseif collection.boxwidth.value == '3/4' %}
                        {% set box_width = '9' %}
                    {% elseif collection.boxwidth.value == '1/3' %}
                        {% set box_width = '4' %}
                    {% elseif collection.boxwidth.value == '2/3' %}
                        {% set box_width = '8' %}
                    {% endif %}
                    <div class="col-md-{{ box_width }} col-sm-4 col-xs-12">
                        <ul class="item-service list-inline-block">
                            {% if collection.boximage.value != '' %}
                            <li>
                                <div class="service-icon">
                                    <a href="{{ collection.link.value }}"><img class="wobble-horizontal" src="{{ image(collection.boximage.value) }}" alt="{{ collection.mainheading.value }}" /></a>
                                </div>
                            </li>
                            {% endif %}
                            <li>
                                <div class="service-info">
                                    {% if collection.mainheading.value != '' %}
                                    <h3 class="title18 font-bold"><a href="#" class="black">{{ collection.mainheading.value }}</a></h3>
                                    {% endif %}
                                    {% if collection.subheading.value != '' %}
                                    <h4 class="title14 transition">{{collection.subheading.value}}</h4>
                                    {% endif %}
                                </div>
                            </li>
                        </ul>
                    </div>
                    {% endfor %}
                </div>
            </div>
            {% endif %}
            <!-- End List Service -->
            <div class="list-special-box">
                <div class="row">
                    {% if footer_data.footerProductSlider.section_state == 'enabled' %}
                    <div class="col-md-4 col-sm-6 col-xs-12" id="section-{{ footer_data.footerProductSlider.section_id }}">
                        <div class="special-box border">
                            {% if footer_data.footerProductSlider.fields.displaysectiontitle.value == '1' %}
                            <h2 class="title18 font-bold">{{ footer_data.footerProductSlider.fields.sectiontitle.value }}</h2>
                            {% endif %}
                            <div class="product-special">
                                <div class="special-slider">
                                    {% if footer_data.footerProductSlider.fields.blockFunction.value == 'getLatestProducts' %}
                                    {% set sliderProducts = getLatestProducts(15, 600, 600) %}
                                    
                                    {% elseif footer_data.footerProductSlider.fields.blockFunction.value == 'getBestSellerProducts' %}
                                        {% set sliderProducts = getBestSellerProducts(15, 600, 600) %}
                                    
                                    {% elseif footer_data.footerProductSlider.fields.blockFunction.value == 'getSpecialProducts' %}
                                        {% set sliderProducts = getSpecialProducts(15, 600, 600) %}
                                    
                                    {% elseif footer_data.footerProductSlider.fields.blockFunction.value == 'getProductsByCategoryId' %}
                                    {% set sliderProducts = getProductsByCategoryId(footer_data.footerProductSlider.fields['categories']['value'], 20, 600, 600) %}
                                    
                                    {% elseif footer_data.footerProductSlider.fields.blockFunction.value == 'getFeaturedProducts' %}
                                        {% set sliderProducts = getFeaturedProducts(footer_data.footerProductSlider.fields['products']['value'], 600, 600) %}
                                    
                                    {% endif %}
                                    
                                    {% set items_count = sliderProducts|length %}
                                    
                                    {% if items_count > 0 %}
                                    <div class="wrap-item navi-bottom" data-navigation="true" data-pagination="false" data-itemscustom="[[0,1]]">
                                        {% for item_product in sliderProducts %}
                                        <div class="item-product">
                                            <div class="product-thumb">
                                                <a href="{{ item_product.href }}" class="product-thumb-link zoom-thumb"><img src="{{ item_product.thumb }}" alt="{{ item_product.name }}" /></a>
                                            </div>
                                            <div class="product-info">
                                                <h3 class="product-title title14"><a href="{{ item_product.href }}">{{ item_product.name }}</a></h3>
                                                {% if ('-1' in item_product.price) or ('-1' in item_product.special) %}
                                                <div class="product-price">
                                                    <ins><span class="clickable" onclick="contact_us('{{ item_product.product_id }}');">{{ lang('button_req_quote') }}</span></ins>
                                                    <div id="enquiry-{{ item_product.product_id }}"  style="display:none">{{ item_product.name }}</div>
                                                </div>
                                                {% else %}
                                                    <div class="product-price">
                                                    {% if item_product.special != '' %}
                                                    {{ item_product.special }}
                                                    <del><span class="title14 silver">{{ item_product.price }}</span></del>
                                                    <ins><span class="title14 color">{{ item_product.special }}</span></ins>
                                                    {% else %}
                                                    <ins><span class="title14 color">{{ item_product.price }}</span></ins>
                                                    {% endif %}
                                                    </div>
                                                {% endif %}
                                                {% if config('config_review_status') == '1'  %}
                                                <div class="product-rate">
                                                    <div class="product-rating"><img src="{{ asset('images/stars-' ~ item_product.rating ~ '.png') }}" alt="{{ lang('text_reviews')|format(reviews_count) }}"/> </div>
                                                </div>
                                                {% endif %}
                                                {% if item_product.short_description %}
                                                <p class="desc">{{ item_product.short_description }}  </p>
                                                {% endif %}
                                            </div>
                                        </div>
                                        {% endfor %}
                                    </div>
                                    {% endif %}
                                </div>
                                {% if footer_data.footerProductSlider.fields.buttonText.value != '' %}
