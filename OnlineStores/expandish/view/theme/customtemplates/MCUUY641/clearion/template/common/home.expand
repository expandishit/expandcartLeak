    <!-- FOOTER -->
    <footer class="site-footer footer-opt-8">
        <div class="container">
            {% set enabled_sections = 0 %}
            {% set links_width = 0 %}
            {% set links_area_width = 0 %}
            {% set social_area_width = 0 %}

            {% if footer_data.firstcolumnlinks.section_state == 'enabled' %}
                {% set enabled_sections = enabled_sections + 1 %}
            {% endif %}
            {% if footer_data.secondcolumnlinks.section_state == 'enabled' %}
                {% set enabled_sections = enabled_sections + 1 %}
            {% endif %}
            {% if footer_data.thirdcolumnlinks.section_state == 'enabled' %}
                {% set enabled_sections = enabled_sections + 1 %}
            {% endif %}

            {% if enabled_sections == 2 %}
                {% set links_width = 6 %}
            {% elseif enabled_sections == 3 %}
                {% set links_width = 4 %}
            {% endif %}

            {% if footer_data.socialcolumnlinks.section_state == 'enabled' %}
                {% if enabled_sections == 0 %}
                    {% set social_area_width = 12 %}
                    {% set links_area_width = 0 %}
                {% elseif enabled_sections == 1 %}
                    {% set social_area_width = 6 %}
                    {% set links_area_width = 6 %}
                {% elseif enabled_sections == 2 %}
                    {% set social_area_width = 5 %}
                    {% set links_area_width = 7 %}
                {% elseif enabled_sections == 3 %}
                    {% set social_area_width = 5 %}
                    {% set links_area_width = 7 %}
                {% endif %}
            {% else %}
                {% if enabled_sections > 0 %}
                    {% set links_area_width = 12 %}
                {% endif %}
            {% endif %}

            {% if links_area_width > 0 or social_area_width > 0 %}
            <div class="footer-column">
                <div class="row">
                    {% if links_area_width > 0 %}
                    <div class="col-md-{{ links_area_width }} col-sm-12">
                        <div class="row">
                            {% if footer_data.firstcolumnlinks.section_state == 'enabled' %}
                            <div class="col-md-{{ links_width }}  col-sm-{{ links_width }}"  id="section-{{ footer_data.firstcolumnlinks.section_id }}">
                                <div class="links">
                                    {% if footer_data.firstcolumnlinks.fields.displaycolumntitle.value == '1' %}
                                    <h3 class="title">{{ footer_data.firstcolumnlinks.fields.columntitle.value }}</h3>
                                    {% endif %}
                                    <ul>
                                        {% for collection in footer_data.firstcolumnlinks.collections %}
                                        <li><a href="{{ collection.link.value }}">{{ collection.linkname.value }}</a></li>
                                        {% endfor %}
                                    </ul>
                                </div>
                            </div>
                            {% endif %}

                            {% if footer_data.secondcolumnlinks.section_state == 'enabled' %}
                                <div class="col-md-{{ links_width }}  col-sm-{{ links_width }}" id="section-{{ footer_data.secondcolumnlinks.section_id }}">
                                    <div class="links">
                                        {% if footer_data.secondcolumnlinks.fields.displaycolumntitle.value == '1' %}
                                            <h3 class="title">{{ footer_data.secondcolumnlinks.fields.columntitle.value }}</h3>
                                        {% endif %}
                                        <ul>
                                            {% for collection in footer_data.secondcolumnlinks.collections %}
                                                <li><a href="{{ collection.link.value }}">{{ collection.linkname.value }}</a></li>
                                            {% endfor %}
                                        </ul>
                                    </div>
                                </div>
                            {% endif %}

                            {% if footer_data.thirdcolumnlinks.section_state == 'enabled' %}
                                <div class="col-md-{{ links_width }}  col-sm-{{ links_width }}" id="section-{{ footer_data.thirdcolumnlinks.section_id }}">
                                    <div class="links">
                                        {% if footer_data.thirdcolumnlinks.fields.displaycolumntitle.value == '1' %}
                                            <h3 class="title">{{ footer_data.thirdcolumnlinks.fields.columntitle.value }}</h3>
                                        {% endif %}
                                        <ul>
                                            {% for collection in footer_data.thirdcolumnlinks.collections %}
                                                <li><a href="{{ collection.link.value }}">{{ collection.linkname.value }}</a></li>
                                            {% endfor %}
                                        </ul>
                                    </div>
                                </div>
                            {% endif %}
                        </div>
                    </div>
                    {% endif %}

                    {% if social_area_width > 0 %}
                    <div class="col-md-{{ social_area_width }} col-sm-12" id="section-{{ footer_data.socialcolumnlinks.section_id }}">
                        <div class="block-social">
                            {% if footer_data.socialcolumnlinks.fields.displaycolumntitle.value == '1' %}
                            <div class="block-title">{{ footer_data.socialcolumnlinks.fields.columntitle.value }}</div>
                            {% endif %}
                            <div class="block-content">
                                {% if footer_data.socialcolumnlinks.fields.facebooklink.value != '' %}
                               <a target="_blank" href="{{ footer_data.socialcolumnlinks.fields.facebooklink.value }}"><i class="fa fa-facebook" aria-hidden="true"></i></a>
                                {% endif %}

                                {% if footer_data.socialcolumnlinks.fields.twitterlink.value != '' %}
                               <a target="_blank" href="{{ footer_data.socialcolumnlinks.fields.twitterlink.value }}"><i class="fa fa-twitter" aria-hidden="true"></i></a>
                                {% endif %}

                                {% if footer_data.socialcolumnlinks.fields.linkedinlink.value != '' %}
                               <a target="_blank" href="{{ footer_data.socialcolumnlinks.fields.linkedinlink.value }}"><i class="fa fa-linkedin" aria-hidden="true"></i></a>
                                {% endif %}

                                {% if footer_data.socialcolumnlinks.fields.googlepluslink.value != '' %}
                               <a target="_blank" href="{{ footer_data.socialcolumnlinks.fields.googlepluslink.value }}"><i class="fa fa-google-plus" aria-hidden="true"></i></a>
                                {% endif %}

                                {% if footer_data.socialcolumnlinks.fields.pinterestlink.value != '' %}
                               <a target="_blank" href="{{ footer_data.socialcolumnlinks.fields.pinterestlink.value }}"><i class="fa fa-pinterest-p" aria-hidden="true"></i></a>
                                {% endif %}

                                {% if footer_data.socialcolumnlinks.fields.youtubelink.value != '' %}
                               <a target="_blank" href="{{ footer_data.socialcolumnlinks.fields.youtubelink.value }}"><i class="fa fa-youtube" aria-hidden="true"></i></a>
                                {% endif %}

                                {% if footer_data.socialcolumnlinks.fields.instagramlink.value != '' %}
                               <a target="_blank" href="{{ footer_data.socialcolumnlinks.fields.instagramlink.value }}"><i class="fa fa-instagram" aria-hidden="true"></i></a>
                                {% endif %}

                                {% if footer_data.socialcolumnlinks.fields.snapchatlink.value != '' %}
                               <a target="_blank" href="{{ footer_data.socialcolumnlinks.fields.snapchatlink.value }}"><i class="fa fa-snapchat" aria-hidden="true"></i></a>
                                {% endif %}

                                {% if footer_data.socialcolumnlinks.fields.whatsapplink.value != '' %}
                               <a target="_blank" href="{{ footer_data.socialcolumnlinks.fields.whatsapplink.value }}"><i class="fa fa-whatsapp" aria-hidden="true"></i></a>
                                {% endif %}
                            </div>
                        </div>
                    </div>
                    {% endif %}
                </div>
            </div>
            {% endif %}

            {% if footer_data.footerimages.section_state == 'enabled' %}
            <div class="payment-methods" id="section-{{ footer_data.footerimages.section_id }}">
                {% for collection in footer_data.footerimages.collections %}
                <a href="{{ collection.imagelink.value }}"><img src="{{ image(collection.image.value) }}" alt="logo"></a>
                {% endfor %}
            </div>
            {% endif %}

            {% if footer_data.firstlinksrow.section_state == 'enabled' or
                  footer_data.secondlinksrow.section_state == 'enabled' or
                  footer_data.thirdlinksrow.section_state == 'enabled'%}
            <div class="footer-bottom">
                <div class="links">
                    {% if footer_data.firstlinksrow.section_state == 'enabled' %}
                        <ul id="section-{{ footer_data.firstlinksrow.section_id }}">
                            {% for collection in footer_data.firstlinksrow.collections %}
                                <li><a href="{{ collection.link.value }}">{{ collection.linkname.value }}</a></li>
                            {% endfor %}
                        </ul>
                    {% endif %}

                    {% if footer_data.secondlinksrow.section_state == 'enabled' %}
                    <ul id="section-{{ footer_data.secondlinksrow.section_id }}">
                        {% for collection in footer_data.secondlinksrow.collections %}
                        <li><a href="{{ collection.link.value }}">{{ collection.linkname.value }}</a></li>
                        {% endfor %}
                    </ul>
                    {% endif %}

                    {% if footer_data.thirdlinksrow.section_state == 'enabled' %}
                    <ul id="section-{{ footer_data.thirdlinksrow.section_id }}">
                        {% for collection in footer_data.thirdlinksrow.collections %}
                        <li><a href="{{ collection.link.value }}">{{ collection.linkname.value }}</a></li>
                        {% endfor %}
                    </ul>
                    {% endif %}
                </div>
            </div>
            {% endif %}

            {% if footer_data.Copyrights.section_state == 'enabled' %}
            <div class="copyright" id="section-{{ footer_data.Copyrights.section_id }}">
                    <div class="row">
                        <div class="col-md-8 col-sm-8 col-xs-12">
                            <p class="copyright text-left">                                
                                {% set Content_Footer = footer_data.Copyrights.fields.Copyrights.value %}
                                {% set replace_value = '[year]' %}											
                                {% set Current_year =  "now"|date('Y')  %}																							
                                {{ Content_Footer|replace({ (replace_value): Current_year}) }}
                            </p>
                        </div>
                        <div class="col-md-4 col-sm-4 col-xs-12">
                            <p class="designby  text-right">
                                {% if PackageId in ['2', '3'] %}
                                    {#<!-- hardcode the copyrights -->#}
                                    {{lang('Copy_Right')}}
                                {% else %}											
                                    {#<!--the copyrights can be editable from the editor -->#}
                                    {{ footer_data.Copyrights.fields.copyrightsPowered.value | raw | html_decode }}
                                {% endif %}										                                
                            </p>
                        </div>
                    </div>
            </div>
            {% endif %}
        </div>
    </footer><!-- end FOOTER -->

