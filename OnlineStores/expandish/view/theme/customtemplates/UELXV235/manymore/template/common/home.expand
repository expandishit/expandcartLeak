<!DOCTYPE html>
<html lang="{{ lang('code') }}" class="{{ lang('direction') == 'rtl' ? 'cms-rtl' : '' }}">
<head>
	<title>{{ document_title }}</title>

	<meta charset="utf-8">
	<meta http-equiv="X-UA-Compatible" content="IE=edge">
	<meta name="viewport" content="width=device-width, initial-scale=1">

	<base href="{{ base_url }}" />
	{% if document_description != '' %}
		<meta name="description" content="{{ document_description }}" />
	{% endif %}

	{% if document_keywords != '' %}
		<meta name="keywords" content="{{ document_keywords }}" />
	{% endif %}

	{% if store_icon != '' %}
		<link href="{{ store_icon }}" rel="icon" />

	{% endif %}

	{% for link in document_links %}
		<link href="{{ link.href }}" rel="{{ link.rel }}" />
	{% endfor %}

	{% for style in document_styles %}
		<link rel="{{ style.rel }}" type="text/css" href="{{ style.href }}" media="{{ style.media }}" />
	{% endfor %}

	{% set store_logo = image(header_data.headermain.fields.logo.value, 600, 600) %}

	{% if route == "product/product" %}
    	{% include 'default/template/structured_data/facebook_og.expand' %}
	{% else %}
		{% include 'default/template/structured_data/store.expand' %}
    {% endif %}

	<!-- Style CSS -->
	<link rel="stylesheet" type="text/css"
	      href="index.php?route=common/style{{ isdraft == 1 ? '&isdraft=1' : '' }}{{ config('tmp_prev') ? '&__p=' ~ config('tmp_prev') : '' }}">

	<script type="text/javascript" src="{{ jslink('js/jquery.min.js') }}"></script>
	<script type="text/javascript" src="expandish/view/theme/abstract/js/rotate360.js"></script>
	<script type="text/javascript" src="{{ jslink('js/jquery-ui.min.js') }}"></script>
	<link rel="stylesheet" type="text/css" href="{{ csslink('css/jquery-ui-1.8.16.custom.css') }}" />
	<link rel="stylesheet" type="text/css" href="{{ csslink('js/jquery/ui/themes/ui-lightness/jquery-ui-1.8.16.custom.css') }}" />



	{% for script in document_scripts %}
		<script type="text/javascript" src="{{ script }}"></script>
	{% endfor %}

	<!-- Inline Script:Start -->
	{% for script in inline_scripts %}
		{% if script['type'] == 'callable' %}
			{{ script['script']|base64_decode }}
		{% endif %}
	{% endfor %}
	<!-- Inline Script:End -->
	<!--[if IE]>
	<style>
		.inner-adv-color-info {
			left:-30px;
			}
	</style>
	<![endif]-->
	{{ google_analytics }}
	{% if lang("direction") != "rtl" %}
		<link rel="stylesheet" type="text/css" href="/expandish/view/theme/abstract/css/menu_mobile.css" />
	{% endif %}
	{% if lang("direction") == "rtl" %}
		<link rel="stylesheet" type="text/css" href="/expandish/view/theme/abstract/css/menu_mobile_RTL.css" />
	{% endif %}
</head>
<body>

{# include top banner app html ("topBannerFixed" parameter used to show fixed layout) #}
{% include "default/template/module/top_banner/index.expand" with {'topBannerFixed': false} %}

{# include Menu for Mobile from Abstract css and js #}
{% include 'manymore/template/common/menu_mobile.expand' %}
<div class="wrap canvas" id="canvas">
	<div id="header">
		<div class="header">
			<div class="top-header top-header4 hidden-xs">
				<div class="container">
					<div class="row">
						<div class="col-md-6 col-sm-6 col-xs-12">
						<div style="
							display: inline-block;
							{{lang('direction') == 'rtl' ? 'float: right;' : 'float: left;'}}
							padding: 10px;
							{{lang('direction') == 'rtl' ? 'border-left: 1px solid #e8e8e8;' : 'border-right: 1px solid #e8e8e8;'}}
						"><a href="#">{{lang('switch_cchyper')}}</a></div>
							<div class="account-login">
								{% if logged == true  %}
									<a href="{{ link('account/account') }}"> {{ lang('Account_Account')  }}</a>
									<a href="{{ link('account/logout', '', 'SSL') }}">{{ lang('AccountLogout')|format(link('account/account', '', 'SSL'), customer.getFirstName(), link('account/logout', '', 'SSL')) }}</a>
								{% endif %}
								{% if logged == false  %}
									{# <a href="{{link('account/login', '', 'SSL')}}">{{ lang('Account_Login')|format(link('account/login', '', 'SSL'), link('account/register', '', 'SSL')) }}</a>
									<a href="{{ link('account/register', '', 'SSL') }}">{{ lang('Account_Register')|format(link('account/login', '', 'SSL'), link('account/register', '', 'SSL')) }}</a>
									{% if multiseller == true %}
										<a href="{{ link('seller/register-seller') }}">{{ lang('text_register_seller') }}</a>
									{% endif %} #}
                                    {% include 'default/template/common/login_link.expand' %}
                                    {% if multiseller == true %}{% include 'default/template/common/login_seller_link.expand' %}{% endif %}
								{% endif %}

								{% if header_data.topHeaderLinKs.section_state == 'enabled' %}
									{% for collection in header_data.topHeaderLinKs.collections %}
										<a href="{{ collection.itemlink.value }}">{{ collection.itemname.value }}</a>
									{% endfor %}
								{% endif %}
							</div>
						</div>
						<div class="col-md-6 col-sm-6 col-xs-12">
							<div class="currency-language">
								{% if languages|length > 1 %}
									<div class="language-box">
										<a href="#" class="language-current" titles="{{ current_language.name }}">
											<img src="admin/view/image/flags/{{ current_language.image }}" alt="{{ current_language.name }}" /><span>{{ current_language.name }}</span>
										</a>
										<form method="post" enctype="multipart/form-data">
											<ul class="language-list list-unstyled">
												{% for language in languages %}
													<li>
														<a href="#" title="{{ language.name }}" onclick="$('input[name=\'language_code\']').attr('value', '{{ language.code }}'); $(this).closest('form').submit(); return false;">
															<img src="admin/view/image/flags/{{ language.image }}" alt="{{ language.name }}" /><span>{{ language.name }}</span>
														</a>
													</li>
												{% endfor %}
												<input type="hidden" name="language_code" value="" />
												<input type="hidden" name="redirect" value="{{ redirect }}" />
											</ul>
										</form>
									</div>
								{% endif %}
								{% if currencies|length > 1 %}
									<div class="currency-box">
										<a href="#" class="currency-current"><span>{{ current_currency.symbol }} - {{ current_currency.title }}</span></a>
										<form action="{{ link('common/home') }}" method="post" enctype="multipart/form-data">
											<ul class="currency-list list-unstyled">
												{% for currency in currencies %}
													<li>
														<a href="#" onclick="$('input[name=\'currency_code\']').attr('value', '{{ currency.code }}'); $(this).closest('form').submit(); return false;">
															<span class="currency-index">{{ currency.symbol }}</span>
														</a>
													</li>
												{% endfor %}
											</ul>
											<input type="hidden" name="currency_code" value="" />
											<input type="hidden" name="redirect" value="{{ redirect }}" />
										</form>
									</div>
								{% endif %}
							</div>
						</div>
					</div>
				</div>
			</div>
			<!-- End Top Header -->
			<div class="main-header">
				<div class="container">
					<div class="row">

						<div class="col-md-3 col-sm-3 col-xs-12 hidden-xs">
							{% if header_data.headermain.section_state == 'enabled' %}
								<div class="logo logo4">
									<h1 class="hidden">{{ store_name }}</h1>
									<a href="{{ link('common/home') }}">
										<img src="{{ image(header_data.headermain.fields.logo.value) }}" title="{{ store_name }}" alt="{{ store_name }}" style="height: {{ header_data.headermain.fields.logoheight.value }}px;"/>
									</a>
								</div>
							{% endif %}
						</div>
						<div class="col-md-6 col-sm-6 col-xs-12 navWrapper hidden-xs">
							<div class="smart-search smart-search4"  style="padding-left: 0px;">
								<div class="form-search">
									<form class="smart-search-form ajax-search">
										<div class="box-group" id="search">
											<input type="text" name="search" class="form-control" placeholder="{{ lang('text_search') }}" value="{{ search }}">
											<button class="btn btn-search button-search" type="button"><span class="hide">{{ lang('text_searchbtn') }}</span></button>
										</div>
									</form>
								</div>
							</div>
						</div>
						<div class="col-md-3 col-sm-3 col-xs-6 cartWrapper">
							<div class="check-cart check-cart4">
							  {% if customer.isCustomerAllowedToAdd_cart %}
								<div class="mini-cart-box block-minicart">
									<a class="mini-cart-link" href="{{ link('checkout/cart') }}">
										<span class="mini-cart-icon"><i class="fa fa-shopping-basket" aria-hidden="true"></i></span>
										<span class="mini-cart-number counter-label" id="counterLabel">{{ cart.countProducts() }}</span>
									</a>
									<ul class="list-mini-cart-item list-unstyled " id="cartDropList">
										{{ cart_content }}
									</ul>
								</div>
								 {% endif %}

								<!-- End Mini Cart -->

								<div class="wishlist-box">
									<a href="{{ link('account/wishlist') }}" class="wishlist-top-link" title="{{ lang('text_wishlist')|format(wishlist_count) }}">
										<i class="fa fa-heart-o" aria-hidden="true"></i>
										<span id="wishlist-total">{{  wishlist_count  }}</span>
									</a>
								</div>
								<div class="checkout-box">
									<a href="#" class="checkout-link"><i class="fa fa-user" aria-hidden="true"></i></a>
									<ul class="list-checkout list-unstyled">
										<li><a href="{{ link('account/account') }}"><i class="fa fa-user"></i> {{ lang('Account_Account') }}</a></li>
										<li><a href="{{ link('account/wishlist') }}"><i class="fa fa-heart-o"></i> {{ lang('text_wishlist')|format(wishlist_count)  }}</a></li>
										<li><a href="{{ link('product/compare') }}"><i class="fa fa-refresh"></i> {{ lang('text_compare_to') }}</a></li>
										{% if logged == false  %}
											{# <li><a href="{{ link('account/login') }}"><i class="fa fa-key"></i> {{ lang('Account_Login') }}</a></li>
											<li><a
														href="{{ link('account/register', '', 'SSL') }}"><i class="fa fa-key"></i>{{ lang('Account_Register')|format(link('account/login', '', 'SSL'), link('account/register', '', 'SSL')) }}</a></li> #}
                                            <li>{% include 'default/template/common/login_link.expand' with {with_icon: true, no_white_space: true} %}</li>
                                            {% if multiseller == true %}<li>{% include 'default/template/common/login_seller_link.expand' with {with_icon: true, no_white_space: true} %}</li>{% endif %}
										{% else %}
											<li><a href="{{ link('account/logout') }}"><i class="fa fa-sign-out"></i> {{ lang('Account_Logout') }}</a></li>
										{% endif %}
										<li><a href="{{ link('checkout/checkout') }}"><i class="fa fa-sign-in"></i> {{ lang('text_checkout') }}</a></li>
									</ul>
								</div>
								<!-- End Check Out Box -->
							</div>
						</div>

						<div class="col-md-12 col-sm-12 col-xs-2">
							{% if header_data.mainMenuLink.collections|length > 0 or
								header_data.mainMenuCategories.collections|length > 0  or
								header_data.mainMenuLinkEnd.collections|length > 0 or
								header_data.megaMenuLink.collections|length > 0 or
								header_data.megaMenuLink2.collections|length > 0 %}
								<nav class="main-nav main-nav4">
									<ul>
										{% if header_data.mainMenuLink.section_state == 'enabled' %}
											{% for collection in header_data.mainMenuLink.collections %}
												<li class="">
													<a href="{{ collection.itemlink.value }}">{{ collection.itemname.value }}</a>
												</li>
											{% endfor %}
										{% endif %}
										{% if header_data.megaMenuLink2.section_state == 'enabled' %}
											{% for collection in header_data.megaMenuLink2.collections %}
												<li class="has-mega-menu">
													<a href="{{collection.ColumnLink.value}}">{{ collection.ColumnTitle.value }} </a>
													{% set featured_categories =  getCategoryAndChilds(collection['categories']['value'], 500, 500) %}
													{% set categories_count = featured_categories|length %}

													{% set cat_width = 4 %}
													{% if collection.displaySidead.value == 1   %}
														{% if categories_count == 3 %}
															{% set cat_width = 3 %}
														{% elseif categories_count > 3 %}
															{% set cat_width = 4 %}
														{%endif%}
													{% else %}
														{% if categories_count == 2 %}
															{% set cat_width = 6 %}
														{% elseif categories_count == 3 %}
															{% set cat_width = 4 %}
														{% elseif categories_count == 4 %}
															{% set cat_width = 3 %}
														{% elseif categories_count > 4 %}
															{% set cat_width = 4 %}
														{%endif%}
													{% endif %}


													<div class="mega-menu mobileCat">
														<div class="row">
															{% for item_category in featured_categories %}
																<div class="col-md-{{cat_width}} col-sm-3">
																	<div class="list-cat-mega-menu">
																		<h2 class="title-cat-mega-menu"><a href="{{ item_category.href }}">{{ item_category.name }}</a></h2>
																		<ul>
																			{% for sub_categories in item_category["sub_categories"]|slice(0, 12) %}
																				<li><a href="{{ sub_categories.href }}" title="{{sub_categories.name}}">{{ sub_categories.name }}</a></li>
																			{% endfor %}
																		</ul>
																	</div>
																</div>
															{% endfor %}
															{% if collection.displaySidead.value == 1 %}
																<div class="col-md-{{cat_width}} col-sm-3 hidden-xs">
																	<div class="banner-image">
																		<a href="{{ collection.sidedadlink.value }}"><img src="{{ image(collection.sideAdimg.value) }}"  /></a>
																	</div>
																</div>
															{% endif %}
														</div>
													</div>
												</li>
											{% endfor %}
										{% endif %}
										{% if header_data.megaMenuLink.section_state == 'enabled' %}
											{% for collection in header_data.megaMenuLink.collections %}
												{% set megaMenuProducts = getFeaturedProducts(collection['products']['value'], 400, 400) %}
												<li class="has-mega-menu">
													<a href="{{collection.ColumnLink.value}}">{{ collection.ColumnTitle.value }} </a>
													<div class="mega-menu">
														<div class="row">
															{% if collection.displaySidead.value == 1 %}
																<div class="col-md-5 col-sm-4 col-xs-12">
																	<div class="mega-adv">
																		<div class="banner-image">
																			<a href="{{ collection.sidedadlink.value }}"><img src="{{ image(collection.sideAdimg.value) }}" alt="{{ collection.sideAdTitle.value }}" /></a>
																		</div>
																		<div class="mega-adv-info">
																			<h3 class="title18"><a href="{{ collection.sidedadlink.value }}">{{ collection.sideAdTitle.value }}</a></h3>
																			<p class="desc">{{ collection.sideAdbody.value }}</p>
																		</div>
																	</div>
																</div>
															{% endif %}
															<div class="{% if collection.displaySidead.value ==1 %} col-md-7 col-sm-8 {%endif%}   col-xs-12">
																<div class="mega-new-arrival">
																	<h2 class="mega-menu-title">{{ collection.ColumnTitle.value }}</h2>
																	<div class="mega-new-arrival-slider">
																		<div class="wrap-item" data-pagination="false" data-navigation="true" data-itemscustom="[[0,1],[768,2]]">
																			{% for product  in megaMenuProducts %}
																				<div class="item">
																					<div class="item-product-ajax item-product">
																						<div class="product-thumb">
																							<a href="{{product.href}}" class="product-thumb-link">
																								<img src="{{product.thumb}}" alt="{{product.name}}">
																							</a>
																							<div class="product-extra-link">
																							  {% if customer.isCustomerAllowedToAdd_cart and ( config('config_hide_add_to_cart') != 1 or product.quantity >0 )  %}
																								<a  href="#" onclick="event.preventDefault(); addToCart({{ product.product_id }}, 1, {{ ('-1' in product.price) or ('-1' in product.special) ? 'true' : 'false' }}); return false;"  class="addcart-link"><i class="fa fa-shopping-basket" aria-hidden="true"></i></a>
																								 {% endif %}
																								<div id="enquiry-{{ product.product_id }}"  style="display:none">{{ product.name }}:</div>
																								<a href="#" onclick="event.preventDefault(); addToWishList('{{ product.product_id }}',1); return false;" class="wishlist-link"><i class="fa fa-heart" aria-hidden="true"></i></a>
																								<a href="#"  onclick="event.preventDefault(); addToCompare('{{ product.product_id }}',1); return false;" class="compare-link"><i class="fa fa-refresh" aria-hidden="true"></i></a>
																							</div>
																						</div>
																						<div class="product-info {{ config('show_brands') ? 'fix-height3' : '' }}">
																							<h3 class="product-title"><a href="{{ product.href }}">{{ product.name }}</a></h3>
																							{% if config('show_brands') and product.manufacturer %}
																								<h5 class="product-title"><a href="{{ product.manufacturer_href }}" id="brand_sections" alt"{{ product.manufacturer }}">{{ product.manufacturer }}</a></h5>
																							{% endif %}
																							<div class="product-price">
																								{% if ('-1' in product.price) or ('-1' in product.special) %}
																									<ins><span class="clickable" onclick="contact_us('{{ product.product_id }}');">{{ lang('button_req_quote') }}</span></ins>
																									<div id="enquiry-{{ product.product_id }}"  style="display:none">{{ product.name }}:</div>
																								{% elseif product.special %}
																									<del><span>{{product.price}}</span></del>
																									<ins><span>{{product.special}}</span></ins>
																								{% else %}
																									<ins><span>{{product.price}}</span></ins>
																								{% endif %}
																							</div>
																						</div>
																					</div>
																				</div>
																			{% endfor %}
																			<!-- End Item -->
																		</div>
																	</div>
																</div>
															</div>
														</div>
													</div>
													<!-- End Mega Menu -->
												</li>
											{% endfor %}
										{% endif %}
										{% if header_data.mainMenuCategories.section_state == 'enabled' %}
											{% for collection in header_data.mainMenuCategories.collections %}
												{% set menu_item_category = getCategoryAndChilds(collection['categories']['value'], 170, 100) %}
												<li class="menu-item-has-children">
													<a href="{{ collection.itemlink.value }}">{{ collection.itemname.value }}</a>
													<ul class="sub-menu">
														{% for menu_category in menu_item_category %}
															{% set subLength =  menu_category["sub_categories"]|length %}
															{% for sub_category in menu_category["sub_categories"]|slice(0,8) %}
																<li class="">
																	<a href="{{ sub_category.href }}">{{ sub_category.name }}</a>
																</li>
															{% endfor %}
															{% if subLength > 8	 %}
																<li><a href="{{ menu_category.href }}"><small>{{ lang('button_view_all') }}</small> {{ lang('direction') == 'rtl' ? '<i class="fa-chevron-circle-left fa muted"></i>' : '<i class="fa-chevron-circle-right fa muted"></i>' }}  </a></li>
															{% endif %}
														{% endfor %}
													</ul>
												</li>
											{% endfor %}
										{% endif %}
										{% if header_data.mainMenuLinkEnd.section_state == 'enabled' %}
											{% for collection in header_data.mainMenuLinkEnd.collections %}
												<li class="">
													<a href="{{ collection.itemlink.value }}">{{ collection.itemname.value }}</a>
												</li>
											{% endfor %}
										{% endif %}
									</ul>
									<a href="#" data-toggle="push" data-target="#sidebar-left" data-distance="{{ lang('direction') == 'rtl' ? '-250' : '250' }}" title="Open navigation" class="toggle-mobile-menu"><span></span></a>
								</nav>
								<!-- End Main Nav -->
							{% endif %}
						</div>
						<div class="col-md-3 col-sm-3 col-xs-4 visible-xs">
							{% if header_data.headermain.section_state == 'enabled' %}
								<div class="logo logo4 navbar-header">
									<h1 class="hidden">{{ store_name }}</h1>
									<a href="{{ link('common/home') }}" class="navbar-brand">
										<img src="{{ image(header_data.headermain.fields.logo.value) }}" class="img-responsive"
										     title="{{ store_name }}"
										     alt="{{ store_name }}" style="height: {{ header_data.headermain.fields.logoheight.value }}px;" />
									</a>
								</div>
							{% endif %}
						</div>
						<div class="col-md-3 col-sm-3 col-xs-6 rtl-show">
							<div class="check-cart check-cart4">
							{% if customer.isCustomerAllowedToAdd_cart %}
								<div class="mini-cart-box block-minicart">
									<a class="mini-cart-link" href="{{ link('checkout/cart') }}">
										<span class="mini-cart-icon"><i class="fa fa-shopping-basket" aria-hidden="true"></i></span>
										<span class="mini-cart-number counter-label" id="counterLabel">{{ cart.countProducts() }}</span>
									</a>
									<ul class="list-mini-cart-item list-unstyled " id="cartDropList">
										{{ cart_content }}
									</ul>
								</div>
								 {% endif %}
								<!-- End Mini Cart -->

								<div class="wishlist-box">
									<a href="{{ link('account/wishlist') }}" class="wishlist-top-link"
									   title="{{ lang('text_wishlist')|format(wishlist_count) }}">
										<i class="fa fa-heart-o" aria-hidden="true"></i>
										<span id="wishlist-total">{{  wishlist_count  }}</span>
									</a>
								</div>
								<div class="checkout-box">
									<a href="#" class="checkout-link"><i class="fa fa-user" aria-hidden="true"></i></a>
									<ul class="list-checkout list-unstyled">
										<li><a href="{{ link('account/account') }}"><i class="fa fa-user"></i> {{ lang('Account_Account') }}</a>
										</li>
										<li><a href="{{ link('account/wishlist') }}"><i class="fa fa-heart-o"></i>
												{{ lang('text_wishlist')|format(wishlist_count)  }}</a></li>
										<li><a href="{{ link('product/compare') }}"><i class="fa fa-refresh"></i>
												{{ lang('text_compare_to') }}</a></li>
										{% if logged == false  %}
											{# <li><a href="{{ link('account/login') }}"><i class="fa fa-key"></i> {{ lang('Account_Login') }}</a></li>
											<li><a href="{{ link('account/register', '', 'SSL') }}"><i
															class="fa fa-key"></i>{{ lang('Account_Register')|format(link('account/login', '', 'SSL'), link('account/register', '', 'SSL')) }}</a>
											</li> #}
                                            <li>{% include 'default/template/common/login_link.expand' with {with_icon: true, no_white_space: true} %}</li>
                                            {% if multiseller == true %}<li>{% include 'default/template/common/login_seller_link.expand' with {with_icon: true, no_white_space: true} %}</li>{% endif %}
										{% else %}
											<li><a href="{{ link('account/logout') }}"><i class="fa fa-sign-out"></i> {{ lang('Account_Logout') }}</a>
											</li>
										{% endif %}
