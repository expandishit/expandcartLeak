<script type="text/javascript">
    function contact_us(id) {
        $('#contact-form input[name="enquiry"]').val($('#enquiry-'+id).html()+'\n');
        $('#contact-form input[name="product_id"]').val(id);
        $('#contact-form').submit();
    }
