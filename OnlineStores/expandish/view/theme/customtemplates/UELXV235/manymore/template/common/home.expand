				<!-- End Footer Tab -->
				<div class="footer-copyright" id="section-{{ footer_data.copyrights.section_id is defined?footer_data.copyrights.section_id: footer_data.Copyrights.section_id }}">
					<div class="container">
						<div class="row">
							<div class="col-md-8 col-sm-8 col-xs-12">
								<p class="copyright">

									{% set Content_Footer = footer_data.copyrights.fields.copyrights.value is defined?footer_data.copyrights.fields.copyrights.value: footer_data.Copyrights.fields.Copyrights.value %}
									{% set replace_value = '[year]' %}
									{% set Current_year =  "now"|date('Y')  %}
									{{ Content_Footer|replace({ (replace_value): Current_year}) }}
								</p>
							</div>
							<div class="col-md-4 col-sm-4 col-xs-12">
								<p class="designby">
									{% if PackageId in ['2', '3'] %}
										{#<!-- hardcode the copyrights -->#}
										{{lang('Copy_Right')}}
									{% else %}
										{#<!--the copyrights can be editable from the editor -->#}
										{{ footer_data.copyrights.fields.copyrightsPowered.value is defined?footer_data.copyrights.fields.copyrightsPowered.value: footer_data.Copyrights.fields.copyrightsPowered.value}}
									{% endif %}

								</p>
							</div>
						</div>
					</div>
				</div>
			{% endif %}
			<!-- End Footer Copyright -->

		</div>
	</div>
	<!-- End Footer -->
	<a href="#" class="radius scroll-top"><i class="fa fa-angle-up" aria-hidden="true"></i></a>
</div>

