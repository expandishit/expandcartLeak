<!DOCTYPE html>
<html lang="{{ lang('code') }}" class="{{ lang('direction') == 'rtl' ? 'cms-rtl' : '' }}">
<head>
    <title>{{ document_title }}</title>

    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">

    <base href="{{ base_url }}" />
    {% if document_description != '' %}
    <meta name="description" content="{{ document_description }}" />
    {% endif %}

    {% if document_keywords != '' %}
    <meta name="keywords" content="{{ document_keywords }}" />
    {% endif %}

    {% if store_icon != '' %}
    <link href="{{ store_icon }}" rel="icon" />
    {% endif %}

    <!-- Web Fonts  -->
    {% if ssl is defined and ssl == 'SSL' %}
      {% set http_word = 'https' %}
    {% else %}
      {% set http_word = 'http' %}
    {% endif %}
    <link href="{{ http_word }}://fonts.googleapis.com/css?family=Open+Sans:300,400,600,700,800%7CShadows+Into+Light" rel="stylesheet" type="text/css">

    {% for link in document_links %}
        <link href="{{ link.href }}" rel="{{ link.rel }}" />
    {% endfor %}

    {% for style in document_styles %}
        <link rel="{{ style.rel }}" type="text/css" href="{{ style.href }}" media="{{ style.media }}" />
    {% endfor %}


    {% if route == "product/product" %}
      {% include 'default/template/structured_data/facebook_og.expand' %}
    {% else %}
      {% include 'default/template/structured_data/store.expand' %}
    {% endif %}
  <link rel = "stylesheet"
        href = "https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.13.1/css/all.min.css">
    <!-- Style CSS -->
    <link rel="stylesheet" type="text/css"
        href="index.php?route=common/style{{ isdraft == 1 ? '&isdraft=1' : '' }}{{ config('tmp_prev') ? '&__p=' ~ config('tmp_prev') : '' }}">

    <!--[if IE]>
            <link rel="stylesheet" href="css/ie.css">
            <![endif]-->
    <!--[if lte IE 8]>
        <script src="http://cdnjs.cloudflare.com/ajax/libs/respond.js/1.3.0/respond.js"></script>
        <script src="http://cdnjs.cloudflare.com/ajax/libs/html5shiv/3.7/html5shiv.js"></script>
        <![endif]-->

    <!-- Modernizr -->
    <script src="{{ jslink('vendor/modernizr/modernizr.js') }}"></script>

    <script src="{{ jslink('vendor/jquery/jquery.js') }}"></script>
    <script type="text/javascript" src="{{ jslink('js/jquery-ui.min.js') }}"></script>
    {% if route == "product/product" %}
      <script type="text/javascript" src="expandish/view/javascript/jquery/ui/jquery-ui-timepicker-addon.js"></script>
    {% endif %}
    <link rel="stylesheet" type="text/css" href="{{ csslink('css/jquery-ui-1.8.16.custom.css') }}" />
    <link rel="stylesheet" type="text/css" href="{{ csslink('js/jquery/ui/themes/ui-lightness/jquery-ui-1.8.16.custom.css') }}" />

    <link rel="stylesheet" type="text/css" href="{{ csslink('vendor/rs-plugin/css/settings.css') }}" media="screen" />

    <!-- jQuery 1.10.1-->
    <!-- <script src="{{ jslink('vendor/jquery/jquery.js') }}"></script>
      <script src="{{ jslink('js/jquery-ui.min.js') }}"></script> -->
      {% for script in document_scripts %}
      <script type="text/javascript" src="{{ script }}"></script>
      {% endfor %}
      <script type="text/javascript" src="expandish/view/theme/abstract/js/rotate360.js"></script>
      <!-- Inline Script:Start -->
      {% for script in inline_scripts %}
      {% if script['type'] == 'callable' %}
      {{ script['script']|base64_decode }}
      {% endif %}
      {% endfor %}

  <!-- Inline Script:End -->

    {{ google_analytics }}

    <script src="{{ jslink('vendor/slick/slick.min.js') }}"></script>
  <link rel="stylesheet" type="text/css" href="/expandish/view/theme/abstract/css/libs/push.css" />
  <link rel="stylesheet" type="text/css" href="/expandish/view/theme/abstract/css/menu_mobile.css" />
  {% if lang("direction") == "rtl" %}
    <link rel="stylesheet" type="text/css" href="/expandish/view/theme/abstract/css/menu_mobile_RTL.css" />
  {% endif %}
</head>
<body class="{{ templatesettings.layout['layout'] }}">

{# include top banner app html ("topBannerFixed" parameter used to show fixed layout) #}
{% include "default/template/module/top_banner/index.expand" with {'topBannerFixed': false} %}

{# include Menu for Mobile from Abstract css and js #}
{% include 'welldone/template/common/menu_mobile.expand' %}
<div class="wrap canvas" id="canvas">
  <div id="loader-wrapper" class="loader-on">
    <div id="loader"></div>
  </div>
  <!-- Modal Search -->
  <div class="overlay overlay-scale">
    <button type="button" class="overlay-close"> âœ• </button>
    <div class="overlay__content">
      <form id="search-form" class="search-form outer" action="#" method="post">
        <div class="input-group input-group--wd" id="search">
          <input type="text" name="search" class="input--full" placeholder="{{ lang('text_search') }}" value="{{ search }}">
          <span class="input-group__bar"></span> </div>
        <button class="btn btn--wd text-uppercase wave waves-effect button-search" type="button">{{ lang('text_searchbtn') }}</button>
      </form>
    </div>
  </div>
  <!-- / end Modal Search -->

  <div class="wrapper">
    <!-- Header section -->
    <header class="header header--sticky">
      {% if header_data.topbar.section_state == 'enabled' %}
      <div class="header-line hidden-xs" style="background-color:{{ templatesettings.colors['fixedHeaderBG'] }}">
        <div class="container">
          {% if header_data.topbar.fields.displaySocialLinksHeader.value == '1' %}
          <div class="pull-left">
            <div class="social-links social-links--colorize">
              <ul>
                  {% if header_data.topbar.fields.facebooklink.value != '' %}
									<li class="social-links__item"><a target="_blank" href="{{ header_data.topbar.fields.facebooklink.value }}" class="icon icon-facebook" aria-hidden="true"></a></li>
									{% endif %}

									{% if header_data.topbar.fields.twitterlink.value != '' %}
									<li class="social-links__item"><a target="_blank" href="{{ header_data.topbar.fields.twitterlink.value }}" class="icon icon-twitter" aria-hidden="true"></a></li>
									{% endif %}

									{% if header_data.topbar.fields.linkedinlink.value != '' %}
									<li class="social-links__item"><a target="_blank" href="{{ header_data.topbar.fields.linkedinlink.value }}" class="icon icon-linkedin" aria-hidden="true"></a></li>
									{% endif %}

									{% if header_data.topbar.fields.googlepluslink.value != '' %}
									<li class="social-links__item"><a target="_blank" href="{{ header_data.topbar.fields.googlepluslink.value }}" class="icon icon-google" aria-hidden="true"></a></li>
									{% endif %}

									{% if header_data.topbar.fields.pinterestlink.value != '' %}
									<li class="social-links__item"><a target="_blank" href="{{ header_data.topbar.fields.pinterestlink.value }}" class="icon icon-pinterest" aria-hidden="true"></a></li>
									{% endif %}

									{% if header_data.topbar.fields.youtubelink.value != '' %}
									<li class="social-links__item"><a target="_blank" href="{{ header_data.topbar.fields.youtubelink.value }}" class="icon icon-youtube" aria-hidden="true"></a></li>
									{% endif %}

									{% if header_data.topbar.fields.instagramlink.value != '' %}
									<li class="social-links__item"><a target="_blank" href="{{ header_data.topbar.fields.instagramlink.value }}" class="icon icon-instagram" aria-hidden="true"></a></li>
									{% endif %}
									
                  {% if header_data.topbar.fields.whatsapplink.value != '' %}
									<li class="social-links__item">
                    <a href="{{ header_data.topbar.fields.whatsapplink.value }}" target="_blank" aria-hidden="true">
                      <i class="fa fa-whatsapp"></i>
                    </a>
                  </li>
									{% endif %}
                {% if header_data.topbar.fields.tiktoklink.value != '' %}
                  <li class="social-links__item">
                    <a href="{{ header_data.topbar.fields.tiktoklink.value }}" target="_blank" aria-hidden="true">
                      <i class="fab fa-tiktok"></i>
                    </a>
                  </li>
                {% endif %}
                  
                  {% if header_data.topbar.fields.snapchatlink.value != '' %}
									<li class="social-links__item">
                    <a href="{{ header_data.topbar.fields.snapchatlink.value }}" target="_blank" aria-hidden="true">
                      <i class="fa fa-snapchat-square"></i>
                    </a>
                  </li>
									{% endif %}
               </ul>
            </div>
          </div>
          {% endif %}
          <div class="pull-right">
            <div class="user-links">
              <ul>
                {% if logged == false  %}
                  {# <li class="user-links__item"><a href="{{ link('account/register') }}">{{ lang('text_register') }}</a></li>
                  <li class="user-links__item"><a href="{{ link('account/login') }}">{{ lang('text_login_welldone') }}</a></li>
                  {% if multiseller == true %}
                  <li class="user-links__item"><a href="index.php?route=seller/register-seller">{{ lang('text_register_seller') }}</a></li>
                    {% endif %} #}
                    <li class="user-links__item">{% include 'default/template/common/login_link.expand' %}</li>
                    {% if multiseller == true %}<li class="user-links__item">{% include 'default/template/common/login_seller_link.expand' %}</li>{% endif %}
                {% else %}
                  <li class="user-links__item"><a href="{{ link('account/account') }}">{{ lang('text_account')  }}</a></li>
                  <li class="user-links__item"><a href="{{ link('account/logout') }}">{{ lang('text_logout') }}</a></li>
                {% endif %}
                <li class="user-links__item"><a href="{{ link('checkout/checkout') }}">{{ lang('text_checkout') }}</a></li>

              </ul>
            </div>
          </div>
        </div>
      </div>
      {% endif %}


      <div class="header__dropdowns-container">
        <div class="header__dropdowns">
          <div class="header__search pull-left">
            <a href="#" class="btn dropdown-toggle btn--links--dropdown header__dropdowns__button search-open">
            <span class="icon icon-search"></span></a>
          </div>
          {% if customer.isCustomerAllowedToAdd_cart %}
          <div class="header__cart pull-left">
            <!-- <span class="header__cart__indicator hidden-xs">$0.00</span> -->
            {# <div class="dropdown pull-right" id="cartDropList"> #}
            <div class="dropdown pull-right cartDropList">
              <a href="#" class="btn dropdown-toggle btn--links--dropdown header__cart__button header__dropdowns__button" data-toggle="dropdown">
                <span class="icon icon-bag-alt"></span>
                <span class="badge badge--menu" id="counterLabel">{{ cart.countProducts() }}</span>
              </a>
              <div class="dropdown-menu animated fadeIn shopping-cart" role="menu">
              {{ cart_content }}
              </div>
            </div>
          </div>
          {% endif %}

          <div class="dropdown pull-right">
            <a href="#" class="btn dropdown-toggle btn--links--dropdown header__dropdowns__button" data-toggle="dropdown">
              <span class="icon icon-dots"></span>
            </a>
            <ul class="dropdown-menu ul-row animated fadeIn" role="menu">
              {% if currencies|length > 1 %}
              <li class='li-col currency'>
                <h4>{{ lang('text_currancy') }} - {{ current_currency.symbol }}</h4>
                <form action="{{ link('common/home') }}" method="post" enctype="multipart/form-data">
                  <ul>
                    <!--<li class="currency__item active"><a href="#"><span>{{ current_currency.symbol }}</span>{{ current_currency.title }}</a></li>-->
                    {% for currency in currencies %}
                    <li class="currency__item">
                      <a href="#" onclick="$('input[name=\'currency_code\']').attr('value', '{{ currency.code }}'); $(this).closest('form').submit(); return false;">
                        <span class="currency-index"><span> {{ currency.symbol }}</span>{{currency.title}}</span>
                      </a>
                    </li>
                    {% endfor %}
                  </ul>
                  <input type="hidden" name="currency_code" value="" />
                  <input type="hidden" name="redirect" value="{{ redirect }}" />
                </form>
              </li>
              {% endif %}
              {% if languages|length > 1 %}
              <li class='li-col languages languages--flag'>
                <h4>{{ lang('text_language') }}</h4>
                <form method="post" enctype="multipart/form-data">
                  <ul>
                    {% for language in languages %}
                    <li class="languages__item">
                      <a href="#" title="{{ language.name }}" onclick="$('input[name=\'language_code\']').attr('value', '{{ language.code }}'); $(this).closest('form').submit(); return false;">
                        <span class="languages__item__flag flag">
                          <img src="admin/view/image/flags/{{ language.image }}" alt="{{ language.name }}"/>
                        </span>
                        <span class="languages__item__label">{{ language.name }}</span>
                      </a>
                    </li>
                    {% endfor %}
                    <input type="hidden" name="language_code" value="" />
                  	<input type="hidden" name="redirect" value="{{ redirect }}" />
                  </ul>
                </form>
              </li>
              {% endif %}
              <li class='li-col list-user-menu'>
                <h4>{{ lang('text_account') }}</h4>
                <ul>
                  {% if logged == false  %}
                    {# <li><a href="{{ link('account/account') }}">{{ lang('text_account') }}</a></li> #}
                    <li>{% include 'default/template/common/login_link.expand' %}</li>
                    {% if multiseller == true %}<li>{% include 'default/template/common/login_seller_link.expand' %}</li>{% endif %}
                  {% else %}
                    <li><a href="{{ link('account/account') }}">{{ lang('text_account')  }}</a></li>
                    <li><a href="{{ link('account/logout') }}">{{ lang('text_logout') }}</a></li>
                  {% endif %}
                  <li><a href="{{ link('account/wishlist') }}">{{ lang('text_wishlist')|format(wishlist_count)  }}</a></li>
                  <li><a href="{{ link('product/compare') }}">{{ lang('text_compare_to') }}</a></li>
                </ul>
              </li>
            </ul>
          </div>
        </div>
      </div>
      <nav class="navbar navbar-wd" id="navbar">
