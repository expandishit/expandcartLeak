<!DOCTYPE html>
<html lang="{{ lang('code') }}" class="{{ lang('direction') == 'rtl' ? 'cms-rtl' : '' }}">
<head>
    <title>{{ document_title }}</title>

    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">

    <base href="{{ base_url }}" />
    {% if document_description != '' %}
    <meta name="description" content="{{ document_description }}" />
    {% endif %}

    {% if document_keywords != '' %}
    <meta name="keywords" content="{{ document_keywords }}" />
    {% endif %}

    {% if store_icon != '' %}
    <link href="{{ store_icon }}" rel="icon" />
    {% endif %}

    <!-- Web Fonts  -->
    <link href="https://fonts.googleapis.com/css?family=Open+Sans:300,400,600,700,800%7CShadows+Into+Light" rel="stylesheet" type="text/css">

    {% for link in document_links %}
        <link href="{{ link.href }}" rel="{{ link.rel }}" />
    {% endfor %}

    {% for style in document_styles %}
        <link rel="{{ style.rel }}" type="text/css" href="{{ style.href }}" media="{{ style.media }}" />
    {% endfor %}

    {% if route == "product/product" %}
      {% include 'default/template/structured_data/facebook_og.expand' %}
    {% else %}
    {% include 'default/template/structured_data/store.expand' %}
    {% endif %}

    <!-- Style CSS -->
  <link rel="stylesheet" type="text/css"
        href="index.php?route=common/style{{ isdraft == 1 ? '&isdraft=1' : '' }}{{ config('tmp_prev') ? '&__p=' ~ config('tmp_prev') : '' }}">

    <!--[if IE]>
            <link rel="stylesheet" href="css/ie.css">
            <![endif]-->
    <!--[if lte IE 8]>
        <script src="http://cdnjs.cloudflare.com/ajax/libs/respond.js/1.3.0/respond.js"></script>
        <script src="http://cdnjs.cloudflare.com/ajax/libs/html5shiv/3.7/html5shiv.js"></script>
        <![endif]-->

    <!-- Modernizr -->
    <script src="{{ jslink('vendor/modernizr/modernizr.js') }}"></script>

    <script type="text/javascript" src="{{ jslink('js/jquery.min.js') }}"></script>
<script type="text/javascript" src="expandish/view/theme/abstract/js/rotate360.js"></script>
    <script type="text/javascript" src="{{ jslink('js/jquery-ui.min.js') }}"></script>
    {% if route == "product/product" %}
      <script type="text/javascript" src="expandish/view/javascript/jquery/ui/jquery-ui-timepicker-addon.js"></script>
    {% endif %}
    <link rel="stylesheet" type="text/css" href="{{ csslink('js/jquery/ui/themes/ui-lightness/jquery-ui-1.8.16.custom.css') }}" />

    <link rel="stylesheet" type="text/css" href="{{ csslink('vendor/rs-plugin/css/settings.css') }}" media="screen" />

    {% for script in document_scripts %}
        <script type="text/javascript" src="{{ script }}"></script>
    {% endfor %}

  <!-- Inline Script:Start -->
    {% for script in inline_scripts %}
        {% if script['type'] == 'callable' %}
            {{ script['script']|base64_decode }}
        {% endif %}
    {% endfor %}
  <!-- Inline Script:End -->

    {{ google_analytics }}

    	<!-- Gameball -->
    {% if gameballApiKey %}

    		<script>
    		{% if logged == false  %}
                window.GbLoadInit = function(){
                    GbSdk.init({
                        lang: '{{ lang('code') }}',
                        APIKey: '{{ gameballApiKey }}'
                    });
                };
              {% else %}
              window.GbLoadInit = function(){
                      GbSdk.init({
                          playerUniqueId: '{{ customer.getId() }}',
                          playerAttributes: {
                              displayName: '{{ customer.getFirstName() }}',
                              email: '{{ customer.getEmail() }}'
                          },
                          lang: '{{ lang('code') }}',
                          APIKey: '{{ gameballApiKey }}'
                      });
                  };
              {% endif %}
            </script>

            <script defer src="https://assets.gameball.co/widget/js/gameball-init.min.js"></script>
        {% endif %}

</head>
<body class="{{ templatesettings.layout['layout'] }}">

  {# include top banner app html ("topBannerFixed" parameter used to show fixed layout) #}
  {% include "default/template/module/top_banner/index.expand" with {'topBannerFixed': false} %}

  <div id="loader-wrapper" class="loader-on">
    <div id="loader"></div>
  </div>
  <!-- Modal Search -->
  <div class="overlay overlay-scale">
    <button type="button" class="overlay-close"> ✕ </button>
    <div class="overlay__content">
      <form id="search-form" class="search-form outer" action="#" method="post">
        <div class="input-group input-group--wd" id="search">
          <input type="text" name="search" class="input--full" placeholder="{{ lang('text_search') }}" value="{{ search }}">
          <span class="input-group__bar"></span> </div>
        <button class="btn btn--wd text-uppercase wave waves-effect button-search" type="button">{{ lang('text_searchbtn') }}</button>
      </form>
    </div>
  </div>
  <!-- / end Modal Search -->

  <div class="wrapper">
    <!-- Header section -->
    <header class="header header--sticky">
      {% if header_data.topbar.section_state == 'enabled' %}
      <div class="header-line hidden-xs" style="background-color:{{ templatesettings.colors['fixedHeaderBG'] }}">
        <div class="container">
          {% if header_data.topbar.fields.displaySocialLinksHeader.value == '1' %}
          <div class="pull-left">
            <div class="social-links social-links--colorize">
              <ul>
                  {% if header_data.topbar.fields.facebooklink.value != '' %}
									<li class="social-links__item"><a href="{{ header_data.topbar.fields.facebooklink.value }}" class="icon icon-facebook" aria-hidden="true"></a></li>
									{% endif %}

									{% if header_data.topbar.fields.twitterlink.value != '' %}
									<li class="social-links__item"><a href="{{ header_data.topbar.fields.twitterlink.value }}" class="icon icon-twitter" aria-hidden="true"></a></li>
									{% endif %}

									{% if header_data.topbar.fields.linkedinlink.value != '' %}
									<li class="social-links__item"><a href="{{ header_data.topbar.fields.linkedinlink.value }}" class="icon icon-linkedin" aria-hidden="true"></a></li>
									{% endif %}

									{% if header_data.topbar.fields.googlepluslink.value != '' %}
									<li class="social-links__item"><a href="{{ header_data.topbar.fields.googlepluslink.value }}" class="icon icon-google" aria-hidden="true"></a></li>
									{% endif %}

									{% if header_data.topbar.fields.pinterestlink.value != '' %}
									<li class="social-links__item"><a href="{{ header_data.topbar.fields.pinterestlink.value }}" class="icon icon-pinterest" aria-hidden="true"></a></li>
									{% endif %}

									{% if header_data.topbar.fields.youtubelink.value != '' %}
									<li class="social-links__item"><a href="{{ header_data.topbar.fields.youtubelink.value }}" class="icon icon-youtube" aria-hidden="true"></a></li>
									{% endif %}

									{% if header_data.topbar.fields.instagramlink.value != '' %}
									<li class="social-links__item"><a href="{{ header_data.topbar.fields.instagramlink.value }}" class="icon icon-instagram" aria-hidden="true"></a></li>
									{% endif %}
               </ul>
            </div>
          </div>
          {% endif %}
          <div class="pull-right">
            <div class="user-links">
              <ul>
                {% if logged == false  %}
                  <li class="user-links__item">
                    {% include 'default/template/common/login_link.expand' with {default_label: 'text_login_welldone'} %}
                 </li>
                 
                 {% if is_enabled_new_login == false %}
                    <li class="user-links__item">
                        <a href="{{ link('account/register') }}">{{ lang('text_register') }}</a>
                    </li>
                 {% endif %}
                 
                 {% if(multiseller) %}
                    <li class="user-links__item">
                        {% include 'default/template/common/login_seller_link.expand' %}
                    </li>
                 {% endif %}
                {% else %}
                  <li class="user-links__item"><a href="{{ link('account/logout') }}">{{ lang('text_logout') }}</a></li>
                {% endif %}
                <li class="user-links__item"><a href="{{ link('checkout/checkout') }}">{{ lang('text_checkout') }}</a></li>

              </ul>
            </div>
          </div>
        </div>
      </div>
      {% endif %}


      <div class="header__dropdowns-container">
        <div class="header__dropdowns">
          <div class="header__search pull-left">
            <a href="#" class="btn dropdown-toggle btn--links--dropdown header__dropdowns__button search-open">
            <span class="icon icon-search"></span></a>
          </div>
         {% if customer.isCustomerAllowedToAdd_cart %}
          <div class="header__cart pull-left">
            <!-- <span class="header__cart__indicator hidden-xs">$0.00</span> -->
            <div class="dropdown pull-right cartDropList">
              <a href="#" class="btn dropdown-toggle btn--links--dropdown header__cart__button header__dropdowns__button" data-toggle="dropdown">
                <span class="icon icon-bag-alt"></span>
                <span class="badge badge--menu" id="counterLabel">{{ cart.countProducts() }}</span>
              </a>
              <div class="dropdown-menu animated fadeIn shopping-cart" role="menu">
              {{ cart_content }}
              </div>
            </div>
          </div>
          {% endif %}
          {% include 'default/template/module/customer_notifications.expand' %}

          <div class="dropdown pull-right">
            <a href="#" class="btn dropdown-toggle btn--links--dropdown header__dropdowns__button" data-toggle="dropdown">
              <span class="icon icon-dots"></span>
            </a>
            <ul class="dropdown-menu ul-row animated fadeIn" role="menu">
              {% if currencies|length > 1 %}
              <li class='li-col currency'>
                <h4>{{ lang('text_currancy') }} - {{ current_currency.symbol }}</h4>
                <form action="{{ link('common/home') }}" method="post" enctype="multipart/form-data">
                  <ul>
                    <!--<li class="currency__item active"><a href="#"><span>{{ current_currency.symbol }}</span>{{ current_currency.title }}</a></li>-->
                    {% for currency in currencies %}
                    <li class="currency__item">
                      <a href="#" onclick="$('input[name=\'currency_code\']').attr('value', '{{ currency.code }}'); $(this).closest('form').submit(); return false;">
                        <span class="currency-index"><span> {{ currency.symbol }}</span>{{currency.title}}</span>
                      </a>
                    </li>
                    {% endfor %}
                  </ul>
                  <input type="hidden" name="currency_code" value="" />
                  <input type="hidden" name="redirect" value="{{ redirect }}" />
                </form>
              </li>
              {% endif %}
              {% if languages|length > 1 %}
              <li class='li-col languages languages--flag'>
                <h4>{{ lang('text_language') }}</h4>
                <form method="post" enctype="multipart/form-data">
                  <ul>
                    {% for language in languages %}
                    <li class="languages__item">
                      <a href="#" title="{{ language.name }}" onclick="$('input[name=\'language_code\']').attr('value', '{{ language.code }}'); $(this).closest('form').submit(); return false;">
                        <span class="languages__item__flag flag">
                          <img src="admin/view/image/flags/{{ language.image }}" alt="{{ language.name }}"/>
                        </span>
                        <span class="languages__item__label">{{ language.name }}</span>
                      </a>
                    </li>
                    {% endfor %}
                    <input type="hidden" name="language_code" value="" />
                  	<input type="hidden" name="redirect" value="{{ redirect }}" />
                  </ul>
                </form>
              </li>
              {% endif %}
              <li class='li-col list-user-menu'>
                <h4>{{ lang('text_account') }}</h4>
                <ul>
                  <li><a href="{{ link('account/account') }}">{{ lang('text_account') }}</a></li>
                   {% if customer.isCustomerAllowedToView_products %}
                  <li><a href="{{ link('account/wishlist') }}">{{ lang('text_wishlist')|format(wishlist_count)  }}</a></li>
                  <li><a href="{{ link('product/compare') }}">{{ lang('text_compare_to') }}</a></li>
                  {% endif %}
                </ul>
              </li>
            </ul>
          </div>
        </div>
      </div>
      <nav class="navbar navbar-wd" id="navbar">
        <div class="container">
          <div class="navbar-header">
            <button type="button" class="navbar-toggle" id="slide-nav"> <span class="icon-bar"></span> <span class="icon-bar"></span> <span class="icon-bar"></span> </button>
            
            <!--  Logo  -->
            {% if header_data.StoreLogo.section_state == 'enabled' %}
            <a class="logo" href="{{ link('common/home') }}">
              <img class="logo-default" src="{{ image(header_data.StoreLogo.fields.logo.value) }}" title="{{ store_name }}" alt="{{ store_name }}" style="height: {{ header_data.StoreLogo.fields.logoheight.value }}px;"/>
              <img class="logo-mobile" src="{{ image(header_data.StoreLogo.fields.logoMobile.value) }}" title="{{ store_name }}" alt="{{ store_name }}"/>
            </a>
            {% endif %}
            <!-- End Logo -->

          </div>
          {% if header_data.mainMenuLink.collections|length > 0 or
                header_data.mainMenuDropdown1.collections|length > 0  or
                header_data.mainMenuDropdown2.collections|length > 0  or
                header_data.mainMenuLinkEnd.collections|length > 0 or
                header_data.megaMenuLink.collections|length > 0 %}
          <div class="pull-left search-focus-fade" id="slidemenu">
            <div class="slidemenu-close visible-xs">✕</div>
            <ul class="nav navbar-nav">
                {% if header_data.mainMenuLink.section_state == 'enabled' %}
                {% for collection in header_data.mainMenuLink.collections %}
                <li><a href="{{ collection.itemlink.value }}" class="wave"><span class="link-name">{{ collection.itemname.value }}</span></a></li>
                {% endfor %}
                {% endif %}

                {% if header_data.mainMenuDropdown1.fields.link.value != '' and header_data.mainMenuDropdown1.section_state == 'enabled' %}
                <li>
                  <a href="{{ header_data.mainMenuDropdown1.fields.link.value}}"  class="dropdown-toggle">
                  <span class="link-name">{{ header_data.mainMenuDropdown1.fields.linkname.value}}</span><span class="caret caret--dots"></span>
                  </a>
                  <ul class="dropdown-menu animated fadeIn" role="menu">
                    {% for collection in header_data.mainMenuDropdown1.collections %}
                    <li><a href="{{collection.link.value}}">{{collection.linkname.value}}</a></li>
                    {% endfor %}
                  </ul>
                </li>
                {% endif %}

                {% if header_data.mainMenuDropdown2.fields.link.value != '' and header_data.mainMenuDropdown2.section_state == 'enabled'  %}
                <li>
                  <a href="{{ header_data.mainMenuDropdown2.fields.link.value}}"  class="dropdown-toggle">
                  <span class="link-name">{{ header_data.mainMenuDropdown2.fields.linkname.value}}</span><span class="caret caret--dots"></span>
                  </a>
                  <ul class="dropdown-menu animated fadeIn" role="menu">
                    {% for collection in header_data.mainMenuDropdown2.collections %}
                    <li><a href="{{collection.link.value}}">{{collection.linkname.value}}</a></li>
                    {% endfor %}
                  </ul>
                </li>
                {% endif %}

                {% if header_data.megaMenuLink.fields.itemname.value != '' and header_data.megaMenuLink.section_state == 'enabled' %}
                <li class="menu-large">
                  <a href="{{header_data.megaMenuLink.fields.itemlink.value }}" class="dropdown-toggle">
                    <span class="link-name">{{ header_data.megaMenuLink.fields.itemname.value }}</span><span class="caret caret--dots"></span>
                  </a>
                  <div class="dropdown-menu megamenu animated fadeIn">
                    <div class="container">
                      <ul class="megamenu__columns">
                        {% if header_data.megaMenuLink.fields.displayIconicLinks.value == '1' %}
                        <li class="megamenu__columns__top-block text-uppercase">
                          <ul>
                            {% if header_data.megaMenuLink.fields.iconicSlogan1.value != '' %}
                            <li><span class="{{header_data.megaMenuLink.fields.iconicLinkIcon1.value}}"></span><a href="{{header_data.megaMenuLink.fields.iconicLink1.value}}">{{header_data.megaMenuLink.fields.iconicSlogan1.value}}</a></li>
                            {% endif %}
                            {% if header_data.megaMenuLink.fields.iconicSlogan2.value != '' %}
                            <li><span class="{{header_data.megaMenuLink.fields.iconicLinkIcon2.value}}"></span><a href="{{header_data.megaMenuLink.fields.iconicLink2.value}}">{{header_data.megaMenuLink.fields.iconicSlogan2.value}}</a></li>
                            {% endif %}
                          </ul>
                        </li>
                        {% endif %}
                        {% set featured_categories = getCategoryAndChilds(header_data.megaMenuLink.fields['categories']['value'], 300, 300) %}
                        {% for category in featured_categories %}
                        <li class="level-menu level1">
                          <ul>
                              <li class="title"><a href="{{category.href}}">{{category.name}}</a></li>
                              {% set subLength =  category["sub_categories"]|length %}
                              {% for sub_category in category["sub_categories"]|slice(0, 5) %}
                              <li class="level2"><a href="{{sub_category.href}}">{{ sub_category.name }}</a></li>
                              {% endfor %}
                              {% if subLength > 4 %}
                                <li class="level2"><a href="{{category.href}}">{{ lang('moreCat') }}</a></li>
                              {% endif %}
                          </ul>
                        </li>
                        {% endfor %}
                        {% if header_data.megaMenuLink.fields.displayIconicLinks.value ==  '1' %}
                        <li class="megamenu__columns__bottom-block text-uppercase">
                          <ul>
                            {% if header_data.megaMenuLink.fields.iconicSlogan3.value != '' %}
                            <li><span class="{{header_data.megaMenuLink.fields.iconicLinkIcon3.value}}"></span><a href="{{header_data.megaMenuLink.fields.iconicLink3.value}}">{{header_data.megaMenuLink.fields.iconicSlogan3.value}}</a></li>
                            {% endif %}
                            {% if header_data.megaMenuLink.fields.iconicSlogan4.value != '' %}
                            <li><span class="{{header_data.megaMenuLink.fields.iconicLinkIcon4.value}}"></span><a href="{{header_data.megaMenuLink.fields.iconicLink4.value}}">{{header_data.megaMenuLink.fields.iconicSlogan4.value}}</a></li>
                            {% endif %}
                          </ul>
                        </li>
                        {% endif %}
                        {% if header_data.megaMenuLink.fields.sideImage.value !='' %}
                        <li class="megamenu__columns__side-image"><img src="{{ image(header_data.megaMenuLink.fields.sideImage.value) }}" alt="{{header_data.megaMenuLink.fields.itemname.value}}"> </li>
                        {% endif %}
                      </ul>
                    </div>
                  </div>
                </li>
                {% endif %}

                  
                {# Check if Auctions App is installed, Then display the top menu bar element #}
                {% if auctions_app_installed == true %}
                <li><a href="{{ link('module/auctions') }}" class="wave"><span class="link-name">{{ lang('text_auctions') }}</span></a></li>
                {% endif %}

                {% if form_builder_app_installed == true %}
                <li><a href="{{ link('module/form_builder') }}" class="wave"><span class="link-name">{{form_builder_title}}</span></a></li>
                {% endif %}

                {% if header_data.mainMenuLinkEnd.section_state == 'enabled' %}
                {% for collection in header_data.mainMenuLinkEnd.collections %}
                <li><a href="{{ collection.itemlink.value }}" class="wave"><span class="link-name">{{ collection.itemname.value }}</span></a></li>
                {% endfor %}
                {% endif %}

              </ul>
          </div>
        </div>
        {% endif %}
      </nav>

      {% if modules.socialslides.enabled %}{{ modules.socialslides.content }}{% endif %}
      {% if modules.smartlook.enabled %}{{ modules.smartlook.content }}{% endif %}
      {% if modules.zopim_live_chat.enabled %}{{ modules.zopim_live_chat.content }}{% endif %}
      {% if modules.popupwindow.enabled %}{{ modules.popupwindow.content }}{% endif %}
    </header>
    <!-- End Header section -->

    <div id="pageContent" class="page-content">

      {% if is_home_page %}
      
        {% if category_droplist %}
          {{ include('default/template/common/category_droplist_filter.expand') }}
        {% endif %}
        
        <div id="notification" class="homeNotifications"></div>
				{{ regions_data.maincontent.content }}
				{% elseif route starts with 'account/' %}
			<div class="container no-padding-xs">
				{{ page_content }}
			</div>
		{% else %}
      <div class="innerpages">
          {{ page_content }}


      </div>
			{% endif %}



      {{ regions_data.bottomcontent.content }}

    </div>

    {% if integration_settings['mn_criteo_status'] %}
		  {{ include('manymore/template/module/criteo/criteo_home.expand') }}
	  {% endif %}
    <footer class="footer">
      <div class="footer__links hidden-xs">
        <div class="container">
          <div class="row">
            {% if footer_data.footerLinksLeft.section_state == 'enabled' %}
            <div class="col-sm-6 pull-left" id="section-{{ footer_data.footerLinksLeft.section_id }}">
              <div class="h-links-list pull-left">
                <ul>
                  {% for collection in footer_data.footerLinksLeft.collections %}
                  <li><a href="{{ collection.link.value }}">{{ collection.linkname.value }}</a></li>
                  {% endfor %}
                </ul>
              </div>
            </div>
            {% endif %}
            {% if footer_data.footerLinksRight.section_state == 'enabled' %}
            <div class="col-sm-6 pull-right" id="section-{{ footer_data.footerLinksRight.section_id }}">
              <div class="h-links-list text-right pull-right">
                  {% for collection in footer_data.footerLinksRight.collections %}
                  <li><a href="{{ collection.link.value }}">{{ collection.linkname.value }}</a></li>
                  {% endfor %}
              </div>
            </div>
            {% endif %}
          </div>
        </div>
      </div>
      <div class="footer__column-links"  style="background-color:{{ templatesettings.colors['footerBG'] }}">
        <div class="back-to-top"> <a href="#top" class="btn btn--round btn--round--lg"><span class="icon-arrow-up"></span></a></div>
        <div class="container">
          <div class="row">
            {% if footer_data.footerBio.section_state == 'enabled' %}
            <div class="col-md-3 hidden-xs hidden-sm" id="section-{{ footer_data.footerBio.section_id }}">
              <!--  Logo  -->
              <a class="logo logo--footer" href="{{ footer_data.footerBio.fields.biolink.value }}">
                <img src="{{ image(footer_data.footerBio.fields.bioImage.value) }}" alt=""/>
              </a>
              <!-- End Logo -->
              <p>{{ footer_data.footerBio.fields.bio.value }} </p>
            </div>
            {% endif %}
            {% if footer_data.firstcolumnlinks.section_state == 'enabled' %}
            <div class="col-sm-3 col-md-2 mobile-collapse" id="section-{{ footer_data.firstcolumnlinks.section_id }}">
              {% if footer_data.firstcolumnlinks.fields.displaycolumntitle.value == '1' %}
              <h5 class="title text-uppercase mobile-collapse__title">{{ footer_data.firstcolumnlinks.fields.columntitle.value }} </h5>
              {% endif %}
              <div class="v-links-list mobile-collapse__content">
                <ul>
                  {% for collection in footer_data.firstcolumnlinks.collections %}
                    <li><a href="{{ collection.link.value }}">{{ collection.linkname.value }}</a></li>
                  {% endfor %}
                </ul>
              </div>
            </div>
            {% endif %}
            {% if footer_data.secondcolumnlinks.section_state == 'enabled' %}
            <div class="col-sm-3 col-md-2 mobile-collapse" id="section-{{ footer_data.secondcolumnlinks.section_id }}">
              {% if footer_data.secondcolumnlinks.fields.displaycolumntitle.value == '1' %}
              <h5 class="title text-uppercase mobile-collapse__title">{{ footer_data.secondcolumnlinks.fields.columntitle.value }} </h5>
              {% endif %}
              <div class="v-links-list mobile-collapse__content">
                <ul>
                  {% for collection in footer_data.secondcolumnlinks.collections %}
                    <li><a href="{{ collection.link.value }}">{{ collection.linkname.value }}</a></li>
                  {% endfor %}
                </ul>
              </div>
            </div>
            {% endif %}
            {% if footer_data.thirdcolumnlinks.section_state == 'enabled' %}
            <div class="col-sm-3 col-md-2 mobile-collapse" id="section-{{ footer_data.thirdcolumnlinks.section_id }}">
              {% if footer_data.thirdcolumnlinks.fields.displaycolumntitle.value == '1' %}
              <h5 class="title text-uppercase mobile-collapse__title">{{ footer_data.thirdcolumnlinks.fields.columntitle.value }} </h5>
              {% endif %}
              <div class="v-links-list mobile-collapse__content">
                <ul>
                  {% for collection in footer_data.thirdcolumnlinks.collections %}
                    <li><a href="{{ collection.link.value }}">{{ collection.linkname.value }}</a></li>
                  {% endfor %}
                </ul>
              </div>
            </div>
            {% endif %}
            {%  if footer_data.storecontacts.section_state == 'enabled' %}
            <div class="col-sm-3 col-md-3 mobile-collapse mobile-collapse--last pull-right" id="section-{{ footer_data.storecontacts.section_id }}">
              {% if footer_data.storecontacts.fields.displaycolumntitle.value == '1'%}
              <h5 class="title text-uppercase mobile-collapse__title">{{ footer_data.storecontacts.fields.columntitle.value }}</h5>
              {% endif %}
              <div class="v-links-list mobile-collapse__content">
                <ul>
