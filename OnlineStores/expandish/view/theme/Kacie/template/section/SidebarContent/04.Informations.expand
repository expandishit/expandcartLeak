<div class="widget-wrap space-bottom-50">                             
    <ul class="widget-feature no-margin">
        {% for collection in collections %}
            <li> <a href="{{ collection.Info_Link.field_value }}"> <i class="{{ collection.Info_Icon.field_value }}"></i> <span class="feature-des"> {{ collection.Info_Title.field_value }}</span> </a> </li>
        {% endfor %}
    </ul>
</div>