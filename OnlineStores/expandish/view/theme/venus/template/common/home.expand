<!DOCTYPE html>
<html lang="{{ lang('code') }}" class="{{ lang('direction') == 'rtl' ? 'cms-rtl' : '' }}">
<head>
    <title>{{ document_title }}</title>

    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">

    <base href="{{ base_url }}" />
    {% if document_description != '' %}
    <meta name="description" content="{{ document_description }}" />
    {% endif %}

    {% if document_keywords != '' %}
    <meta name="keywords" content="{{ document_keywords }}" />
    {% endif %}

    {% if store_icon != '' %}
    <link href="{{ store_icon }}" rel="icon" />
    {# <meta property="og:image" content="{{ store_icon }}"/> #}

    {% endif %}

	{% if landing_page == null %}
		{% for link in document_links %}
			<link href="{{ link.href }}" rel="{{ link.rel }}" />
		{% endfor %}

		{% for style in document_styles %}
			<link rel="{{ style.rel }}" type="text/css" href="{{ style.href }}" media="{{ style.media }}" />
		{% endfor %}

		{% if route == "product/product" %}
			{% include 'default/template/structured_data/facebook_og.expand' %}
		{% else %}
			{% include 'default/template/structured_data/store.expand' %}
    	{% endif %}

    <!-- Style CSS -->
	<link rel="stylesheet" type="text/css"
		  href="index.php?route=common/style{{ isdraft == 1 ? '&isdraft=1' : '' }}{{ config('tmp_prev') ? '&__p=' ~ config('tmp_prev') : '' }}">

	 {% endif %}

	{# START LANDING PAGE STYLE #}
	{% if landing_page %}

		<link rel="stylesheet" type="text/css" href="{{ csslink('css/libs/owl.carousel/owl.carousel.css') }}" />
		<link rel="stylesheet" type="text/css"
			href="index.php?route=common/style{{ isdraft == 1 ? '&isdraft=1' : '' }}{{ config('tmp_prev') ? '&__p=' ~ config('tmp_prev') : '' }}">
		<!-- craggy -->
		{% if lang("direction") != "rtl" %}
		<link rel='stylesheet' id='wp-block-library-css' href='{{ csslink('css/craggy/css/style.min6e7a.css')}}'
			type='text/css' media='all' />
		{% endif %}
		{% if lang("direction") == "rtl" %}
		<link rel='stylesheet' id='wp-block-library-rtl-css' href='{{ csslink('css/craggy/css/style-rtl.min6e7a.css') }}'
			type='text/css' media='all' />
		{% endif %}
		<link rel='stylesheet' id='craggy_bootstrap_css-css' href='{{ csslink('css/craggy/css/bootstrap-grid5152.css')}}'
			type='text/css' media='all' />
		<link rel='stylesheet' id='craggy_theme_style-css' href='{{ csslink('css/craggy/css/style9632.css')}}'
			type='text/css' media='all' />
		<link rel='stylesheet' id='gforms_formsmain_css-css' href='{{ csslink('css/craggy/css/formsmain.minf8c7.css')}}'
			type='text/css' media='all' />
		<link rel='stylesheet' id='gforms_ready_class_css-css' href='{{ csslink('css/craggy/css/readyclass.minf8c7.css')}}'
			type='text/css' media='all' />
	
		{% if lang("direction") == "rtl" %}
		<link rel='stylesheet' id='gforms_rtl_css-css' href='{{ csslink('css/craggy/css/rtl.minf8c7.css')}}' type='text/css'
			media='all' />
		{% endif %}
		<link rel='stylesheet' id='craggy_google_fonts-css'
			href='http://fonts.googleapis.com/css?family=Poppins:300,700,800,900%7CMontserrat:800,700,800,900&amp;subset=latin-ext'
			type='text/css' media='all' />
    <!-- end craggy -->
    {% endif %}
	{# END LANDING PAGE STYLE #}

    <script type="text/javascript" src="{{ jslink('js/jquery.min.js') }}"></script>
	<script type="text/javascript" src="expandish/view/theme/abstract/js/rotate360.js"></script>
    <script type="text/javascript" src="{{ jslink('js/jquery-ui.min.js') }}"></script>
    <link rel="stylesheet" type="text/css" href="{{ csslink('css/jquery-ui-1.8.16.custom.css') }}" />
	<link rel="stylesheet" type="text/css" href="{{ csslink('js/jquery/ui/themes/ui-lightness/jquery-ui-1.8.16.custom.css') }}" />

    {% for script in document_scripts %}
        <script type="text/javascript" src="{{ script }}"></script>
    {% endfor %}

	<!-- Inline Script:Start -->
    {% for script in inline_scripts %}
        {% if script['type'] == 'callable' %}
            {{ script['script']|base64_decode }}
        {% endif %}
    {% endfor %}
	<!-- Inline Script:End -->

	<!--[if IE]>
	<style>
		.inner-adv-color-info {
			left:-30px;
		}
	</style>
	<![endif]-->
    {{ google_analytics }}

  	<!-- Gameball -->
  {% if gameballApiKey %}

  		<script>
  		{% if logged == false  %}
              window.GbLoadInit = function(){
                  GbSdk.init({
                      lang: '{{ lang('code') }}',
                      APIKey: '{{ gameballApiKey }}'
                  });
              };
            {% else %}
            window.GbLoadInit = function(){
                    GbSdk.init({
                        playerUniqueId: '{{ customer.getId() }}',
                        playerAttributes: {
                            displayName: '{{ customer.getFirstName() }}',
                            email: '{{ customer.getEmail() }}'
                        },
                        lang: '{{ lang('code') }}',
                        APIKey: '{{ gameballApiKey }}'
                    });
                };
            {% endif %}
          </script>

          <script defer src="https://assets.gameball.co/widget/js/gameball-init.min.js"></script>
      {% endif %}
 {% if landing_page %}
    <link rel='stylesheet' href='{{ csslink('css/craggy/css/customStyle.css')}}' type='text/css' media='all' />
    {% endif %}

</head>
<body class="{{landing_page and lang("direction") == "rtl" ? 'rtl' : ''}} option9">

    {# include top banner app html ("topBannerFixed" parameter used to show fixed layout) #}
    {% include "default/template/module/top_banner/index.expand" with {'topBannerFixed': false} %}

	{% if landing_page == null %}
		<nav class="{{ lang('direction') == 'rtl' ? 'push-sidebar push-sidebar-right' : 'push-sidebar' }} hidden" id="sidebar-left">
			<ul class="nav  nav-stacked sidebar ">
		
				{% if header_data.mainMenuLink.section_state == 'enabled' %}
				{% for collection in header_data.mainMenuLink.collections %}
				<li class="">
					<a href="{{ collection.itemlink.value }}">{{ collection.itemname.value }}</a>
				</li>
				{% endfor %}
				{% endif %}

				{% if header_data.megaMenuLink2.section_state == 'enabled' %}
				{% for collection in header_data.megaMenuLink2.collections %}
				<li role="presentation" class="dropdown">
					<a class="dropdown-toggle" data-toggle="dropdown" href="{{ collection.ColumnLink.value }}" role="button" aria-haspopup="true" aria-expanded="false">
						{{ collection.ColumnTitle.value }} <i class="fa fa-angle-down"></i>
					</a>
					<ul class="dropdown-menu">
					{% set menu_item_category = getCategoryAndChilds(collection['categories']['value'], 170, 100) %}
					{% for menu_category in menu_item_category %}
						{% set cat_sub = menu_category["sub_categories"]|length %}
						{% if  cat_sub == 0 %}
							<li class="">
								<a href="{{ menu_category.href }}">{{menu_category.name }}</a>
							</li>
						{% else %}
							<li role="presentation" class="dropdown subLevel">
								<a class="dropdown-toggle submenu-open" data-toggle="dropdown" href="{{ menu_category.href }}"
								role="button" aria-haspopup="true" aria-expanded="false">
									{{menu_category.name }} <i class="fa fa-angle-down"></i>
								</a>
								<ul class="dropdown-menu">

									{% for sub_category in menu_category["sub_categories"]|slice(0,20) %}
									<li>
										<a href="{{ sub_category.href }}">{{ sub_category.name }}</a>
									</li>
									{% endfor %}

								</ul>
							</li>
						{% endif %}
					{% endfor %}
					</ul>
				{% endfor %}
				{% endif %}

				{% if header_data.mainMenuCategories.section_state == 'enabled' %}
				{% for collection in header_data.mainMenuCategories.collections %}
				{% set menu_item_category = getCategoryAndChilds(collection['categories']['value'], 170, 100) %}
					{% for item_category in menu_item_category %}
						{% set cat_sub = item_category["sub_categories"]|length %}
						{% if  cat_sub == 0 %}
							<li class="">
								<a href="{{ item_category.href }}">{{item_category.name }}</a>
							</li>
						{% else %}
							<li role="presentation" class="dropdown">
								<a class="dropdown-toggle" data-toggle="dropdown" role="button" aria-haspopup="true" aria-expanded="false">
										{{ item_category.name }} <i class="fa fa-angle-down"></i>
								</a>
								<ul class="dropdown-menu">
									<li>
										<a href="{{ item_category.href }}">
										{{ item_category.name }} 
								        </a>
							        </li>
									{% for sub_category in item_category["sub_categories"]|slice(0,9) %}
									<li>
										<a href="{{ sub_category.href }}">{{ sub_category.name }}</a>
									</li>
									{%endfor%}
								</ul>
							</li>
						{% endif %}
					{% endfor %}
				{% endfor %}
				{% endif %}
		
				{% if header_data.mainMenuLinkEnd.section_state == 'enabled' %}
				{% for collection in header_data.mainMenuLinkEnd.collections %}
				<li class="">
					<a href="{{ collection.itemlink.value }}">{{ collection.itemname.value }}</a>
				</li>
				<li role="presentation" class="dropdown">
					<a class="dropdown-toggle" data-toggle="dropdown"  role="button" aria-haspopup="true" aria-expanded="false">
							{{ item_category.name }} <i class="fa fa-angle-down"></i>
					</a>
					<ul class="dropdown-menu">
						<li>
							<a href="{{ item_category.href }}">
							{{ item_category.name }}
							</a>
						</li>
						{% for sub_category in item_category["sub_categories"]|slice(0,9) %}
						<li>
							<a href="{{ sub_category.href }}">{{ sub_category.name }}</a>
						</li>
						{%endfor%}
					</ul>
				</li>
				{% endfor %}
				{% endif %}
		
		
			</ul>
		</nav>
	{% endif %}
	<div class="wrap canvas" id="canvas">
		<div id="header">
			<div class="header {{ landing_page ? 'landing_header':''}}">
			{% if landing_page == null %}
				<div class="top-header top-header4">
					<div class="container">
						<div class="row">
							<div class="col-md-6 col-sm-6 col-xs-12">
								<div class="account-login">
									{% if logged == true  %}
                                        <a href="{{ link('account/account') }}"> {{ lang('Account_Account')  }}</a>
                                        <a href="{{ link('account/logout', '', 'SSL') }}">{{ lang('AccountLogout')|format(link('account/account', '', 'SSL'), customer.getFirstName(), link('account/logout', '', 'SSL')) }}</a>
									{% endif %}
                                    
									{% if logged == false  %}
                                        {# <a href="{{link('account/login', '', 'SSL')}}">{{ lang('Account_Login')|format(link('account/login', '', 'SSL'), link('account/register', '', 'SSL')) }}</a>
                                        <a href="{{ link('account/register', '', 'SSL') }}">{{ lang('Account_Register')|format(link('account/login', '', 'SSL'), link('account/register', '', 'SSL')) }}</a>
                                                                            {% if multiseller == true %}
                                        <a href="{{ link('seller/register-seller', '', 'SSL') }}">{{ lang('text_register_seller')|format(link('account/login', '', 'SSL'), link('seller/register-seller', '', 'SSL')) }}</a>
                                                                            {% endif %} #}
                                        {% include 'default/template/common/login_link.expand' %}
                                        {% if is_enabled_new_login == false %}<a href="{{ link('account/register', '', 'SSL') }}">{{ lang('Account_Register')|format(link('account/login', '', 'SSL'), link('account/register', '', 'SSL')) }}</a>{% endif %}
                                        {% if multiseller == true %}{% include 'default/template/common/login_seller_link.expand' %}{% endif %}
                                    {% endif %}
                                    
									<a href="{{ link('account/wishlist') }}"><i class="fa fa-heart-o"></i> {{ lang('text_wishlist')|format(wishlist_count)  }}</a>
								</div>
							</div>
							<div class="col-md-5 col-sm-5 col-xs-12">
								<div class="currency-language">
									{% if languages|length > 1 %}
									<div class="language-box">
										<a class="language-current" titles="{{ current_language.name }}">
											<img src="admin/view/image/flags/{{ current_language.image }}" alt="{{ current_language.name }}" /><span>{{ current_language.name }}</span>
										</a>
										<form method="post" enctype="multipart/form-data">
											<ul class="language-list list-unstyled">
												{% for language in languages %}
												<li>
													<a href="#" title="{{ language.name }}" onclick="$('input[name=\'language_code\']').attr('value', '{{ language.code }}'); $(this).closest('form').submit(); return false;">
														<img src="admin/view/image/flags/{{ language.image }}" alt="{{ language.name }}" /><span>{{ language.name }}</span>
													</a>
												</li>
												{% endfor %}
												<input type="hidden" name="language_code" value="" />
												<input type="hidden" name="redirect" value="{{ redirect }}" />
											</ul>
										</form>
									</div>
									{% endif %}
									{% if currencies|length > 1 %}
									<div class="currency-box">
										<a href="#" class="currency-current"><span>{{ current_currency.symbol }} - {{ current_currency.title }}</span></a>
										<form action="{{ link('common/home') }}" method="post" enctype="multipart/form-data">
										<ul class="currency-list list-unstyled">
											{% for currency in currencies %}
											<li>
												<a href="#" onclick="$('input[name=\'currency_code\']').attr('value', '{{ currency.code }}'); $(this).closest('form').submit(); return false;">
													<span class="currency-index">{{ currency.symbol }}</span>
												</a>
											</li>
											{% endfor %}
										</ul>
										<input type="hidden" name="currency_code" value="" />
										<input type="hidden" name="redirect" value="{{ redirect }}" />
										</form>
									</div>
									{% endif %}
								</div>
							</div>
							{% include 'default/template/module/customer_notifications.expand' %}
						</div>
					</div>
				</div>
			{% endif %}
				<!-- End Top Header -->
				<div class="main-header1" style="{{ landing_page ? 'background-color:transparent':''}}">
					<div class="container">
						<div class="row">
							
								<div class=" {{ landing_page ? 'col-md-12 text-center landing-logo':'col-md-3 col-sm-3'}}   col-xs-12">
									{% if header_data.headermain.section_state == 'enabled' %}
									<div class="logo logo1 {{ landing_page ? 'logo_landing':''}}"  >
										<h1 class="hidden">{{ store_name }}</h1>
										<a href="{{ link('common/home') }}">
											<img src="{{ image(header_data.headermain.fields.logo.value) }}" title="{{ store_name }}" alt="{{ store_name }}" style="height: {{ header_data.headermain.fields.logoheight.value }}px; max-height:70px;"/>
										</a>
									</div>
									{% endif %}
								</div>
							
							{% if landing_page == null %}
								
								
								<div class="col-md-5 col-sm-6 col-xs-12">
									<div class="wrap-search1">
										<div class="smart-search smart-search1">
											<form class="smart-search-form ajax-search">
												<div class="box-group" id="search">
													<input type="text" name="search" class="form-control" placeholder="{{ lang('text_search') }}" value="{{ search }}">
													<button class="btn btn-search button-search" type="button"><span class="hide">{{ lang('text_searchbtn') }}</span></button>
												</div>
											</form>
										</div>
										<!--
										<div class="search-trend">
											<span>Search trend:</span>
											<a href="#">Fashion </a>
											<a href="#">Mobile</a>
											<a href="#">Smartphone</a>
											<a href="#">Laptop</a>
											<a href="#">Beauty</a>
											<a href="#">Watches</a>
										</div>
									-->
									</div>
								</div>

								<div class="col-md-4 col-sm-3 col-xs-6 cartWrapper">
									<div class="cart-social clearfix">
										<div class="mini-cart-box mini-cart1 block-minicart">
											<a class="mini-cart-link" href="{{ link('checkout/cart') }}">
												<span class="mini-cart-icon"><i class="fa fa-shopping-basket" aria-hidden="true"></i></span>
												<span class="mini-cart-number counter-label" id="counterLabel">{{ cart.countProducts() }}</span>
											</a>
											<ul class="list-mini-cart-item list-unstyled " id="cartDropList">
												{{ cart_content }}
											</ul>
										</div>
										<!-- End Mini Cart -->
										{% if header_data.socialcolumnlinksHeader.section_state == 'enabled' %}
										<div class="social-header">
											{% if header_data.socialcolumnlinksHeader.fields.facebooklink.value != '' %}
											<a href="{{ header_data.socialcolumnlinksHeader.fields.facebooklink.value }}" target="_blank"><i class="fa fa-facebook" aria-hidden="true"></i></a>
											{% endif %}

											{% if header_data.socialcolumnlinksHeader.fields.twitterlink.value != '' %}
											<a href="{{ header_data.socialcolumnlinksHeader.fields.twitterlink.value }}" target="_blank"><i class="fa fa-twitter" aria-hidden="true"></i></a>
											{% endif %}

											{% if header_data.socialcolumnlinksHeader.fields.linkedinlink.value != '' %}
											<a href="{{ header_data.socialcolumnlinksHeader.fields.linkedinlink.value }}" target="_blank"><i class="fa fa-linkedin" aria-hidden="true"></i></a>
											{% endif %}

											{% if header_data.socialcolumnlinksHeader.fields.googlepluslink.value != '' %}
											<a href="{{ header_data.socialcolumnlinksHeader.fields.googlepluslink.value }}" target="_blank"><i class="fa fa-google-plus" aria-hidden="true"></i></a>
											{% endif %}

											{% if header_data.socialcolumnlinksHeader.fields.pinterestlink.value != '' %}
											<a href="{{ header_data.socialcolumnlinksHeader.fields.pinterestlink.value }}" target="_blank"><i class="fa fa-pinterest-p" aria-hidden="true"></i></a>
											{% endif %}

											{% if header_data.socialcolumnlinksHeader.fields.youtubelink.value != '' %}
											<a href="{{ header_data.socialcolumnlinksHeader.fields.youtubelink.value }}" target="_blank"><i class="fa fa-youtube" aria-hidden="true"></i></a>
											{% endif %}

											{% if header_data.socialcolumnlinksHeader.fields.instagramlink.value != '' %}
											<a href="{{ header_data.socialcolumnlinksHeader.fields.instagramlink.value }}" target="_blank"><i class="fa fa-instagram" aria-hidden="true"></i></a>
											{% endif %}

											{% if header_data.socialcolumnlinksHeader.fields.snapchatlink.value != '' %}
											<a href="{{ header_data.socialcolumnlinksHeader.fields.snapchatlink.value }}" target="_blank"><i class="fa fa-snapchat" aria-hidden="true"></i></a>
											{% endif %}

											{% if header_data.socialcolumnlinksHeader.fields.whatsapplink.value != '' %}
											<a href="{{ header_data.socialcolumnlinksHeader.fields.whatsapplink.value }}" target="_blank"><i class="fa fa-whatsapp" aria-hidden="true"></i></a>
											{% endif %}
										</div>
										<!-- End Social -->
										{% endif %}
									</div>
								</div>

								<div class="col-md-12 col-sm-12 col-xs-6 navWrapper">
										{% if header_data.mainMenuLink.collections|length > 0 or
											header_data.mainMenuCategories.collections|length > 0  or
											header_data.mainMenuLinkEnd.collections|length > 0 or
											header_data.megaMenuLink.collections|length > 0 or
											header_data.megaMenuLink2.collections|length > 0 %}
										<nav class="main-nav main-nav3">
											<ul>
												{% if header_data.mainMenuLink.section_state == 'enabled' %}
												{% for collection in header_data.mainMenuLink.collections %}
												<li class="">
													<a href="{{ collection.itemlink.value }}">{{ collection.itemname.value }}</a>
												</li>
												{% endfor %}

												{% endif %}
												{% if header_data.megaMenuLink2.section_state == 'enabled' %}
												{% for collection in header_data.megaMenuLink2.collections %}
												<li class="has-mega-menu">
													<a href="{{collection.ColumnLink.value}}">{{ collection.ColumnTitle.value }} </a>
													{% set featured_categories =  getCategoryAndChilds(collection['categories']['value'], 500, 500) %}

													<div class="mega-menu mobileCat">
														<div class="row">
															{% for item_category in featured_categories %}
															<div class="col-md-4 col-sm-3">
																<div class="list-cat-mega-menu">
																	<h2 class="title-cat-mega-menu"><a href="{{ item_category.href }}">{{ item_category.name }}</a></h2>
																	<ul>
																		{% for sub_categories in item_category["sub_categories"]|slice(0, 12) %}
																		<li><a href="{{ sub_categories.href }}" title="{{sub_categories.name}}">{{ sub_categories.name }}</a></li>
																		{% endfor %}
																	</ul>
																</div>
															</div>
															{% endfor %}
															{% if collection.displaySidead.value == 1 %}
															<div class="col-md-4 col-sm-3 hidden-xs">
																<div class="banner-image">
																	<a href="{{ collection.sidedadlink.href }}"><img src="{{ image(collection.sideAdimg.value) }}"  /></a>
																</div>
															</div>
															{% endif %}
														</div>
													</div>
												</li>
												{% endfor %}
												{% endif %}

												{% if header_data.megaMenuLink.section_state == 'enabled' %}
												{% for collection in header_data.megaMenuLink.collections %}
												{% set megaMenuProducts = getFeaturedProducts(collection['products']['value'], 400, 400) %}
												<li class="has-mega-menu">
													<a href="{{collection.ColumnLink.value}}">{{ collection.ColumnTitle.value }} </a>
													<div class="mega-menu">
														<div class="row">
															{% if collection.displaySidead.value == 1 %}
															<div class="col-md-5 col-sm-4 col-xs-12">
																<div class="mega-adv">
																	<div class="banner-image">
																		<a href="{{ collection.sidedadlink.href }}"><img src="{{ image(collection.sideAdimg.value) }}" alt="{{ collection.sideAdTitle.value }}" /></a>
																	</div>
																	<div class="mega-adv-info">
																		<h3 class="title18"><a href="{{ collection.sidedadlink.href }}">{{ collection.sideAdTitle.value }}</a></h3>
																		<p class="desc">{{ collection.sideAdbody.value }}</p>
																	</div>
																</div>
															</div>
															{% endif %}
															<div class="{% if collection.displaySidead.value ==1 %} col-md-7 col-sm-8 {%endif%}   col-xs-12">
																<div class="mega-new-arrival">
																	<h2 class="mega-menu-title">{{ collection.ColumnTitle.value }}</h2>
																	<div class="mega-new-arrival-slider">
																		<div class="wrap-item" data-pagination="false" data-navigation="true" data-itemscustom="[[0,1],[768,2]]">
																			{% for product  in megaMenuProducts %}
																			<div class="item">
																				<div class="item-product-ajax item-product">
																					<div class="product-thumb">
																						<a href="{{product.href}}" class="product-thumb-link">
																							<img src="{{product.thumb}}" alt="{{product.name}}">
																						</a>
