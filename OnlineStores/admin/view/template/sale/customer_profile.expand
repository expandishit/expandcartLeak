            else
                $('.transcTimeline').fadeOut('slow');
    
        });
        $('#rewardCheck').change(function(){
            if(this.checked)
                $('.rewardTimeline').fadeIn('slow');
            else
                $('.rewardTimeline').fadeOut('slow');
    
        });
        $('#banCheck').change(function(){
            if(this.checked)
                $('.ipTimeline').fadeIn('slow');
            else
