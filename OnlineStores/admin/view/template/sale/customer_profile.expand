        });
        $('#orderCheck').change(function(){
            if(this.checked)
                $('.orderTimeline').fadeIn('slow');
            else
                $('.orderTimeline').fadeOut('slow');
    
        });

        $(".add_new_address").click(function() {
            var clone = $('.blankaddress').clone();
            clone.find("input").val("");
            clone.appendTo('.addressesPanels');
        });
    });
</script>
