                                            <span class="required">*</span>  

                                            </label>
                      
                                <input 
                        
                                    type="text" 
                          
                                        value="{{ reference }}" 
                            
                                            name="zajil_shipment_receiver_reference" 
                              
                                                id="zajil_shipment_receiver_reference" 
                                
                                                    class="form-control">
                                  
                      
                                <span class="help-block"></span>
                      
                            </div>
                            <!-- /Refrence -->
                            <!-- Receiver Name -->
                            <div class="form-group col-md-6" id="zajil_shipment_receiver_name-group">
                               
                                <label for="zajil_shipment_receiver_name" 
                                
                                    class="control-label"> {{ lang('entry_rcvname') }} 
                                    
                                    <span class="required">*</span>  
                                    
                                    </label>
                               
                                        <input 

                                            type="text" 
                          
                                                value="{{ zajil_shipment_receiver_name }}" 
                            
                                                    name="zajil_shipment_receiver_name" 
                                
                                                        id="zajil_shipment_receiver_name" class="form-control">
                                
                                
                               
                                <span class="help-block"></span>
                            
                            </div>
                            <!-- /Receiver Name -->
                            <!-- Receiver Email -->
                            <div class="form-group col-md-6" id="zajil_shipment_receiver_phone-group">
                                
                                <label 
                                
                                    for="zajil_shipment_receiver_phone" 
                                    
                                        class="control-label"> {{ lang('entry_rcvphone') }}
                                        <span class="required">*</span>  

                                         </label>
                                
                                        <input 
                                        
                                            type="text" 
                                            
                                            value="{{ zajil_shipment_receiver_phone }}" 
                                            
                                                name="zajil_shipment_receiver_phone" 
                                                
                                                id="zajil_shipment_receiver_phone" class="form-control">
                                  
                                
                                <span class="help-block"></span>
                            </div>
                            <!-- /Receiver Email -->
                            <!-- Receiver company -->
                            <div class="form-group col-md-6" id="zajil_shipment_receiver_city-group">
                              
                                <label for="zajil_shipment_receiver_city" class="control-label">
                                
                                    {{ lang('entry_rcvcity') }}   

                                    <span class="required">*</span>                                 
                                    
                                    </label>
                              
                                <select name="zajil_shipment_receiver_city" class="form-control" id="shipping-zajil-shipment-receiver-city">
                        
                                {% for city in cities %}
                        
                                    {% if city.value==zajil_shipment_receiver_city %}
                              
                                        <option value="{{ city.value }}" selected="selected">{{ city.text }}</option>
                          
                                    {% else %}
                              
                                        <option value="{{ city.value }}">{{ city.text }}</option>
                          
                                    {% endif %}
                        
                                 {% endfor %}
                        
                                </select>

                                <span class="help-block"></span>
                            
                            </div>
                            <!-- /Receiver company -->
                            <!-- Receiver Country -->
                            
                            <div class="form-group col-md-12" id="zajil_shipment_receiver_street-group">
                            
                                <label for="zajil_shipment_receiver_street" class="control-label">
                                
                                        {{ lang('entry_rcvaddress') }}

                                        <span class="required">*</span>
                                        
                                            </label>

                            
                            <textarea 
                        
                                name="zajil_shipment_receiver_street" 
                            
                                    id="zajil_shipment_receiver_street" 
                              
                                        type="text" 
                                        
                                            cols="26" 
                                            
                                                rows="4" 
                                                
                                                    class="form-control">{{ zajil_shipment_receiver_street }}</textarea>
                                


                                <span class="help-block"></span>
                            </div>
                            {# here here here #}

                            {% if not is_shipment %}

                            <div class="collapse in" id="panel1">
                            
                                <div class="form-group" id="zajil_email_customer-group" style="position:unset;">
                                
                                <label class="control-label">{{ lang('entry_notify_customer') }}</label>
                                
                                <div class="checkbox checkbox-switchery">
                                    
                                    <label>
                                        <input type="checkbox" 
                                        
                                                onchange="changeStatus(this);" 
                                                
                                                    name="zajil_email_customer" 
                                                    
                                                        class="switchery" {{ zajil_email_customer not in ['0', 0, 'off'] and zajil_email_customer is not null ? 'checked' }}>

                                                    <span class="switchery-status">{{ zajil_email_customer not in ['0', 0, 'off'] and zajil_email_customer is not null ? lang('text_enabled') : lang('text_disabled') }}</span>
                                    
                                    </label>
                                
                                </div>
                            
                            </div>

                            {% endif %}

                        </div>
                         
                           
                        
                         
                         
                        </div>
                    </div>
                </div>
            </div>
            

            <!-- Product Information -->

            <div class="col-md-12" id="zajil_items__info">
                
                <div class="panel panel-white">
                
                    <div class="panel-heading">
                
                        <h6 class="panel-title"><span class="text-bold">{{  lang('entry_name')  }}</span></h6>
                
                        <div class="heading-elements not-collapsible">
                
                            <ul class="icons-list">
                
                                <li><a data-action="collapse" class=""></a></li>
                
                            </ul>
                
                        </div>
                
                    </div>
                
                    <div class="panel-body">
                        <div class="collapse in" id="fields-panel">
                           
                            <table class="table col-md-12" style="width: 100%;">
                                
                                <tr>
                                    <th class="aramex_item_name"> {{ lang('entry_name') }} </th>

                                    <th class="aramex_item_qty"> {{ lang('entry_qty') }} </th>
                                
                                </tr>

                                {% set qty=0 %}
                          
                                {% for op in order_products %}
                          
                                <tr class="zajil_item_tobe_shipped" id="item{{ op.order_product_id }}"> 
                            
                                    {# {{ op.name|slice(0, 21) }} #}
                                    <td class="zajil_item_name"><span title="{{ op.name }}"> {{ op.name }} </span>
                          
                                    <input type="hidden" value="{{ op.quantity }}" name="zajil_items[{{ op.order_product_id }}]" id="zajil_items_{{ op.order_product_id }}"></td>
                          

                                    <td class="zajil_item_qty">
                                    
                                        <input type="hidden"   
                                    
                                            value="{{ op.quantity }}" 
                                    
                                                name="{{ op.order_product_id }}" 
                                        
                                                    class="form-control" readonly>
                                     
                                     <label class="label label-success" style="padding: 10px 20px;"> {{ op['quantity'] }} </label>
                          
                                    <input type="hidden" value="{{ op.price }}" name="zajil_items_base_price_{{ op.order_product_id }}" id="zajil_items_base_price_{{ op.order_product_id }}">
                          
                                    <input type="hidden" value="{{ op.weight }}" name="zajil_items_base_weight_{{ op.order_product_id }}" id="zajil_items_base_weight_{{ op.order_product_id }}">
                          
                                    <input type="hidden" value="{{ op.quantity }}" name="zajil_items_total_{{ op.order_product_id }}" id="zajil_items_total_{{ op.order_product_id }}">
                                    
                                    
                                    </td>
                          
                                </tr>
                                
                                {% set qty = qty + op.quantity %}
						          
                                {% endfor %}

                            
                            </table>

                            <div 
                            
                                class="form-group col-md-12 text-center" 
                            
                                    id="zajil_shipment_info_items_subtotal-group">


                                <label for="zajil_shipment_info_foreignhawb" 
                                
                                        class="control-label">{{ lang('entry_number_of_items') }}</label>
                                
                                <br>
                                
                                <input type="hidden" value="{{ qty }}" name="pieces" id="pieces">
                                    
                                
                                <div class="col-md-12">

                                    <label style="width: 100%; height: 30px; vertical-align: middle; line-height: 25px;font-size: 17px;" class="label label-info"> {{ qty }} </label>
                                
                                </div>
                                
                                <span class="help-block"></span>
                            
                            </div>
                            {# done #}
                        </div>
                    </div>
                </div>
            </div>
            <!-- /Product Information -->

            
            <!-- Shippment Information -->
