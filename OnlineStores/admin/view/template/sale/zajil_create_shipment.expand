                                <option id="digital" value="digital" {% if pay_option=='digital' %} selected="selected" {% endif %}>{{ lang('text_digital') }}</option>
                            
                              </select>
                            
                                
                                <span class="help-block"></span>
                            
                            </div>


                      
                           
