                                        </select>
                              
                                    </div>
                                </div>
                                <span class="help-block"></span>
                            </div>
                            <!-- /Total Weight -->

                            <!-- Shippment Information Reference -->
                            <div class="form-group col-md-6" id="zajil_shipment_info_reference-group">
                                
                                <label 
                                
                                    for="zajil_shipment_info_reference" 
                                    
                                        class="control-label">{{ lang('entry_reference') }}
                                        
                                        
                                        </label>
                                
                                <input 
                              
