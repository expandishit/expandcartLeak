                            </div>
                            <!-- /Shipper Email -->
                            <!-- Shipper company -->
                            <div class="form-group col-md-6" id="shipping-zajil-sender-city-group">
                            
                                <label for="shipping-zajil-sender-city" class="control-label">{{ lang('entry_sender_city')  }}</label>
                                
                                <input 
                            
                                    type="text" 
                            
                                      value="{{ zajil_sender_city }}" 
                              
                                        name="zajil_sender_city" id="shipping-zajil-sender-city" class="form-control">
                    

                                        <span class="help-block"></span>
                            
                            </div>
                            <!-- /Shipper company -->
                            <!-- Shipper Country -->
                            <div class="form-group col-md-12" id="zajil-sender-address-group">
                              
                              
                                <label for="zajil-sender-address" class="control-label">
                                
