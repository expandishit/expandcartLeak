                                class="form-group col-md-6" 
                                
                                    id="zajil_shipment_info_payment_option-group">
                            
                                <label for="zajil_shipment_info_payment_option" class="control-label"> {{ lang('entry_payment_option') }} </label>

                                <select name="zajil_shipment_info_payment_option" id="zajil_shipment_info_payment_option" class="form-control">
                            
                                <option value="">{{ lang('text_select') }}</option>                       
                                
                                <option id="cash" value="cash" {% if pay_option=='cash' %} selected="selected" {% endif %}>{{ lang('text_cash') }}</option>
                            
                                <option id="cheque" value="cheque" {% if pay_cheque=='cheque' %} selected="selected" {% endif %}>{{ lang('text_cheque') }}</option>
                            
