{% extends "base.expand" %}

{% from "controls/breadcrumb.expand" import breadcrumb as breadcrumb %}

{% import "controls/forms.expand" as forms %}

{% block title %}

    {{ lang('heading_title') }}

{% endblock title %}

{% block breadcrumb %}

    {{ breadcrumb(breadcrumbs) }}

{% endblock breadcrumb %}



{% block headelements %}

    {% if not is_shipment %}
	 
    <a class="btn btn-info" onclick="$('#zajil_shipment').submit();" 
   
      id="zajil_shipment_creation_submit_id" 
      
        data-toggle="tooltip" 
          
          title="{{ text_create_sipment }}"><i  class="fa fa-ship"></i> {{ lang('text_create_sipment', 'sale/aramex') }}</a>


    {% endif %}
     
    {% if is_shipment %}
	 
   <a href="{{ zajil_print_label }}"  class="btn btn-info" 
   
          data-toggle="tooltip" 
          
            title="{{ text_print_label }}">
            
              <i  class="fa fa-print"></i>
              
                 </a>

	 <a href="{{ zajil_print_tracking }}" 
   
    target="_blank"  
    
      class="btn btn-info" 
      
        data-toggle="tooltip" 
          
          title="{{ text_track }}"><i  class="fa fa-truck"></i> 
          
                 {{ lang('text_track', 'sale/aramex') }}
            
            </a>

      {% endif %}

	  <a href="{{ back_to_order  }}"  
    
          class="btn btn-default" 
          
            data-toggle="tooltip" 
            
                title="{{ text_back_to_order }}">
                
                  <i class="fa fa-reply"></i>

                    {{ lang('text_return_shipment', 'sale/aramex') }}

                    

                     </a>


    {# {% include "includes/buttons_top.expand" %} #}

{% endblock headelements %}


{% block content %}
    <style>
        textarea
        {
            resize: vertical;
        }

        .required{
                color: red;
        }
    </style>

  
 <div class="container-fluid">
   {% if error_warning %}
    <div class="alert alert-danger alert-dismissible"><i class="fa fa-exclamation-circle"></i> {{ error_warning }}
      <button type="button" class="close" data-dismiss="alert">&times;</button>
    </div>
    {% endif %}
    {% if success_html %}
    <div class="alert alert-success alert-dismissible"><i class="fa fa-check-circle"></i> {{ success_html }}
      <button type="button" class="close" data-dismiss="alert">&times;</button>
    </div>
    {% endif %}

        {# ATH page links has been configered #}
  
    
    <!-- Start Form -->
    <form enctype="multipart/form-data" action="" method="post" class="form" id="zajil_shipment" novalidate>

        <input type="hidden" value="" name="zajil_shipment_referer">

          <input type="hidden" value="{{ zajil_password }}" name="zajil_password">
          
          <input type="hidden" value="{{ reference }}" name="zajil_shipment_original_reference">

        <div class="row">
            <!-- Billing Account -->
            <div class="col-md-12" id="aramex_shipment_creation_general_info">
                
                <div class="panel panel-white">
                
                    <div class="panel-heading">
                
                        <h6 class="panel-title">
                        
                            <span 
                                class="text-bold">
                                    
                                  {{ lang('text_billing_account')  }}  
                            
                                </span>
                                
                            </h6>
                
                        <div class="heading-elements not-collapsible">
                
                            <ul class="icons-list">
                
                                <li><a data-action="collapse" class=""></a></li>
                
                            </ul>
                
                        </div>
                
                    </div>
                
                    <div class="panel-body">
                        <div class="collapse in" id="fields-panel">
                            <!-- Account -->
                            <div class="form-group col-md-4" id="zajil_productType-group">
                                
                                <label 
                                
                                    for="zajil_productType" 
                                    
                                        class="control-label">{{ lang('entry_productType') }}</label>

                                <select name="zajil_productType" class="form-control" id="shipping-zajil-productType">
 
                                    {% for productType in productTypes %}


                                        {% if productType.value==zajil_productType %}


                                            <option value="{{ productType.value }}" selected="selected">{{ productType.text }}</option>


                                        {% else %}

                                            <option value="{{ productType.value }}">{{ productType.text }}</option>
            
                                        {% endif %}
          
                                    {% endfor %}
          
                                </select>        
                                
                                {# <input type="text" class="form-control" id="aramex_shipment_shipper_account" name="aramex_shipment_shipper_account" value="{{ aramex_shipment_shipper_account }}"> #}
                                
                                {# <span class="text-muted">{{ text_global_setting_msg }}</span> #}
                                
                                <span class="help-block"></span>
                            
                            </div>
                            <!-- /Account -->
                            <!-- Payment -->
                            
                            <div class="form-group col-md-4" id="zajil_bookingMode-group">
                                
                                <label for="zajil_bookingMode" class="control-label"> {{ lang('entry_bookingMode') }} </label>

                                   <select name="zajil_bookingMode" class="form-control" id="shipping-zajil-bookingMode">
 
 
                                        {% for bookingMode in bookingModes %}
                                        
                                        {% if bookingMode.value==zajil_bookingMode %}

                                        <option value="{{ bookingMode.value }}" selected="selected">{{ bookingMode.text }}</option>
                                        
                                        {% else %}
                                        
                                        
                                        <option value="{{ bookingMode.value }}">{{ bookingMode.text }}</option>
                                        
                                        {% endif %}
                                        
                                        {% endfor %}
                                    
                                    </select>
                                <span class="help-block"></span>
                            </div>

                            <div class="form-group col-md-4" id="zajil_service-group">
                                
                                <label for="zajil_service" class="control-label"> {{ lang('entry_service') }} </label>

                                      <select name="zajil_service" class="form-control" id="shipping-zajil-service">
  
                                        {% for service in services %}
                                        
                                        {% if service.value==zajil_service%}
                                            
                                            <option value="{{ service.value }}" selected="selected">{{ service.description }}</option>

                                        {% else %}
                                            
                                            <option value="{{ service.value }}">{{ service.description }}</option>
                                        
                                            {% endif %}

                                        {% endfor %}

                                        </select>
                                        
                                <span class="help-block"></span>
                            </div>
                            <!-- /Payment -->
                        </div>
                    </div>
                </div>
            </div>
            <!-- /Billing Account -->


            <!-- Shipper Details -->
