{% block content %}
    <div id="modal_order_info" class="modal">
        <div class="modal-dialog modal-dialog-centered">
            <div class="modal-content">
                <div class="modal-header">
                    <h5 class="modal-title" id="exampleModalLabel">{{ lang('text_order_more_details') }}</h5>
                    <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                        <span aria-hidden="true">&times;</span>
                    </button>
                </div>
                <div class="modal-body">
                    <div class="row">
                        <div class="col-md-6">
                            <ul class="list list-icons">
                                {% if user_agent %}
                                    <li>
                                        <i class="icon-browser text-success position-left"></i>
                                        {{ lang('text_user_agent') }}:
                                        <div style="max-width: 420px;overflow-y: hidden;overflow-x: auto;font-weight: bold;"> {{ user_agent }}</div>
                                    </li>
                                    {% if custom_fields['confirm'] %}
                                        {% for fd in custom_fields['confirm'] %}
                                            <li>
                                                <i class="icon-file-text2 text-success position-left"></i>
                                                {{ fd['field_title'] }}: <b> {{ fd['value'] }}</b>
                                            </li>
                                        {% endfor %}
                                    {% endif %}
                                {% endif %}
                            </ul>
                        </div>

                        <div class="col-md-6">
                            <ul class="list list-icons">
                                {% if ip %}
                                    <li>
                                        <i class="icon-link text-success position-left"></i>
                                        {{ lang('text_ip') }}: <b> {{ ip }}</b>
                                    </li>
                                {% endif %}
                                {% if accept_language %}
                                    <li>
                                        <i class="icon-code text-success position-left"></i>
                                        {{ lang('text_accept_language') }}: <b> {{ accept_language }}</b>
                                    </li>
                                {% endif %}

                            </ul>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>

    <!-- Modal -->
    {% if not archived %}
        {% if can_do_action('order_change_status') or can_modify('sale/order') %}
        <div class="modal fade orderStatus-pop" id="updateStatusBtn" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel" aria-hidden="true">
            <div class="modal-dialog" role="document">
                <div class="modal-content">
                    <div class="modal-header">
                        <h6 class="panel-title">
                            <i class="icon-price-tag2 position-left"></i>
                            {{ lang('entry_order_status_modal_title') }}
                        </h6>
                        <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                            <span aria-hidden="true">&times;</span>
                        </button>
