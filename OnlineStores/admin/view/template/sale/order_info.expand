                    confirmButtonColor: "#2196F3",
                    confirmButtonText: "{{ lang('payment_refund_confirm_button') }}",
                    cancelButtonText: "{{ lang('cm_cancel_button') }}"
                });
        });
          var assignOrderVal = $('#assign_order').val();
            $('#assign_order').on('select2:select', (e) => {
                let d = e.params.data;
                let uid = $(d.element).data('unique-id');
                if (typeof uid === 'undefined') {
                    uid = 0;
                }

                confirmMessage(() => {
                    $.ajax({
                        url: assignOrderHref,
                        dataType: 'JSON',
                        method: 'POST',
                        data: {user_id: uid, order_id: orderId},
                        success: (r) => {
                            assignOrderVal = uid;
                        if (typeof r.status !== 'undefined' && r.status === 'OK') {
                            notify('', 'success', "{{lang('assigned_success')}}");
                            return;
                          }
                        }
                    });
                }, () => {
                    $('#assign_order').val(assignOrderVal).change();
                }, {
                    title: "{{ lang('assign_order') }}",
                    text: "{{ lang('ms_assign_order_confirm_body') }}",
                    type: "info",
                    showCancelButton: true,
                    confirmButtonColor: "#2196F3",
                    confirmButtonText: "{{ lang('assign_order_confirm_button') }}",
                    cancelButtonText: locales['cm_cancel_button']
                });
            });
        });
    </script>
{% endblock content %}
