                if ($(d.element).data('bundled') !== 0) {
                    b = $(d.element).data('bundled');
                }

                let uid = $(d.element).data('unique-id');

                let code = false;
                if ($(d.element).data('code') != 'manual') {
                    code = $(d.element).data('code');
                }

                if (typeof uid === 'undefined') {
                    uid = 0;
                }

                confirmMessage(() => {
                    $.ajax({
                        url: updateOrderMsgIdHref,
                        dataType: 'JSON',
                        method: 'POST',
                        data: {id: uid, order_id: orderId, bundled: b, gateway_title: d.text, code: code},
                        success: (r) => {
                            $('#selected_manual_shipping').parent().hide();
                            $('#selected_manual_shipping').html('');
                            if (uid > 0) {
                                $('#selected_manual_shipping').parent().show();
                                $('#selected_manual_shipping').html(d.text);
                            }

                            manualShippingGatewayVal = uid;
                        }
