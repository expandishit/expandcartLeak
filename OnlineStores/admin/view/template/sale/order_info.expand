        $(document).on('click', '#quick-shipment', function() {
                var errors_wrapper_wrapper = $('#errors-wrapper');
                var errors_wrapper = $('#errors-wrapper #errors-content');

                $.ajax({
                    url: "{{ link('shipping/quick_ship/pushOneOrder?order_id=' ~ order_id) }}",
                    type: "POST",
                    beforeSend: function()
                    {
                        startLoadingScreen();
                        errors_wrapper_wrapper.hide();
                        errors_wrapper.html('');
                    },
                    success: function (resp)
                    {
                        removeLoadingScreen();
                        resp = JSON.parse(resp);

                        if ( resp.error )
                        {
                            if ( typeof resp.error == 'object' )
                            {
                                for ( err in resp.error )
                                {
                                    errors_wrapper.append('<li>' + resp.error[err] + '</li>');
                                }
                            }
                            else
                            {
                                errors_wrapper.append('<li>' + resp.error + '</li>');
                            }

                            errors_wrapper_wrapper.show();
                        }
                        else
                        {
                            notify('{{ lang("general_success_text") }}', 'success', resp.success_msg);
                        }
                    }
                });
            });

