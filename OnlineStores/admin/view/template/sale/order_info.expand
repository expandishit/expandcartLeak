{% extends "base.expand" %}
{% from "controls/breadcrumb.expand" import breadcrumb as breadcrumb %}

{% block title %}
    {{ lang('heading_title', 'sale/order') }}
{% endblock title %}

{% block styles %}
    <style type="text/css">
        .pr-warehouse-td .select2-container{
            width: 80% !important;
        }

        .page-header-default .page-header-content + .breadcrumb-line {
            min-height: 40px;
            background-color: #f8f9ff;
            border-bottom: 1px solid #e0ddef;
        }
        .content-wrapper {
            background-color: #F8F9FF;
        }
        .breadcrumb-line .navbar-nav > li + li {
           margin-top: 0;
        }
    </style>
{% endblock styles %}

{% block breadcrumb %}
    {{ breadcrumb(breadcrumbs) }}
{% endblock breadcrumb %}



