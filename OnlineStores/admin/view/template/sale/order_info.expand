                else
                {
                    sms_notif = 0;
                }
                if ( $('#notify_by_whatsapp').is(':checked') )
                {
                    whats_notif = 1;
                }
                else
                {
                    whats_notif = 0;
                }

                if ( $('#notify_by_notifications').is(':checked') )
                {
                    notifications_notif = 1;
                }
                else
                {
                    notifications_notif = 0;
                }

                $.ajax({
                    url: "{{ link('sale/order/history') }}?order_id=" + order_id,
                    type: "POST",
                    data: {notify: notif, notify_by_sms: sms_notif, notify_by_whatsapp: whats_notif, notify_by_notifications: notifications_notif, comment: comment, order_status_id: parseInt(os), 'date_added': "{{ original_date_added }}"},
                    success: function ( resp )
                    {
                        // make post request to get new statuses list after updated
                        const currentOrderStatusId = parseInt($( "#order_status_id option:selected" ).val());
                        // first hide change status button until we load the new status
                        $('.orderLeft-head-btn').hide();
                        // update user order statuses list
                        $.ajax({
                            url : "{{ link('sale/order/userOrderStatuses') }}",
                            type : "POST",
                            data : {
                                current_order_status_id : currentOrderStatusId
                            },
                            success : function ( response ){

                                const statusesList = $('#order_status_id');

                                statusesList.empty();
                                // append new statuses
                                response.forEach(function (status){
                                    if (currentOrderStatusId == status.order_status_id)
                                        statusesList.append(new Option(status.name, status.order_status_id, false , true));
                                    else
                                        statusesList.append(new Option(status.name, status.order_status_id));
                                });

                                // show change status button
                                $('.orderLeft-head-btn').show();
                            },
                            error : function ( response ){
                                // incase any error happen show change status button
                                $('.orderLeft-head-btn').show();
                            }
                        });


                        old_comment = $("#order_comment_block").html();
                        new_comment = $("[name=comment]").val();

                        if(old_comment == ""){
                            $("#order_comment_block").text(new_comment);
                        }
                        else{
                            $("#order_comment_block").html(old_comment+"<br>"+new_comment);
                        }

                        $('#header-order-status').text(os_text);
                        resp = JSON.parse(resp);
                        notify( "Success", 'success', resp.success_msg );

                        lockplan == "true" ? $('#order-histories-wrapper').html( resp.histories) : "";

                        var salasaErrors = resp.salasa_errors;
                        if(resp.salasa_success){
                            $('#salasa_success').fadeIn();
                        }else if(salasaErrors && salasaErrors.length){
                            $('#salasa_errors').html('');
                            $('#salasa_alert').fadeIn();
                            for (var i = 0; i < salasaErrors.length; i++) {
                                $('#salasa_errors').append('<li>'+salasaErrors[i]+'</li>');
                            }
                        }

                        //Catalyst create shipping
                        if(resp.catalyst_data){
                            $('#catalyst_success').fadeOut();
                            $('#catalyst_alert').fadeOut();
                            $('#catalyst_errors').html('');

                            $.post( "sale/catalyst_create_shipment/autoCreateShipment", { cty_data: resp.catalyst_data })
                            .done(function( ctydata ) {
                                ctydata = JSON.parse(ctydata);
                                if(ctydata.success){
                                    $('#catalyst_success').fadeIn();
                                }
                                else if(ctydata.errors){
                                    var cty_errors = '';
                                    $.each(ctydata.errors, function(index, item) {
                                        if(index != 'warning')
                                            cty_errors += '<li>'+item+'</li>';
                                    });
                                    $('#catalyst_alert').fadeIn();
                                    $('#catalyst_errors').html(cty_errors);
                                }
                            });
                        }
                    }
                });
            }, null, confirmdata);

