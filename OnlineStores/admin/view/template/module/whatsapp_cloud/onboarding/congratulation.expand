</div>

<script>
    $(document).ready(function () {
		$('#congratulationSkip').click(function(){
			$(".hide-inprogress").hide();
			$(".show-inprogress").show();
			 congratulationSkip();
		});
    })

function congratulationSkip() {
  $.ajax({
			url: '{{ link('module/whatsapp_cloud/congratulationSkip') }}',
			type: 'post',
			success: function(json_data){
				var data = JSON.parse(json_data);
				if(data.success == '1'){
									window.location = '{{ link('module/whatsapp_cloud') }}';
                   }else{
                            swal({ title:"{{lang('text_something_went_wrong')}}", type: "warning", closeOnConfirm: true  })
                           
			}
			},
			error: function(){
                            swal({ title:"{{lang('text_something_went_wrong')}}", type: "warning", closeOnConfirm: true  })
			}
			});
	
};
</script>
