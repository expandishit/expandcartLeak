				<p class="mt-20 mb-10 m-bold">{{lang('text_congratulations')}}</p>
				<div class="hide-inprogress"> 
					<p class="mb-10">{{lang('text_now_you_can_send_notifications')}}</p>
					<p class="mb-10">{{lang('text_verify_your_WhatsApp_business_number')}}</p>
				</div>
				<div class="show-inprogress" style="display:none;"> 
					<!--<p class="mb-10">{{lang('text_we_are_preparing_you_data')}}</p>-->
					<p class="mb-10">we are preparing you data please wait a seconds .. </p>
				</div>

				<button class="btn btn-primary mb-20 btn-ladda btn-ladda-spinner " id="congratulationSkip">
				<span class="ladda-label hide-inprogress">
					{{lang('text_start_sending_notifications')}} 
					<i class=" fas fa-chevron-right position-right rotate-rtl"></i>
				</span>
				<span class="show-inprogress" style="display:none;">
					 loading .. 
				</span>
				
				<span class="ladda-spinner"></span>
				</button>
