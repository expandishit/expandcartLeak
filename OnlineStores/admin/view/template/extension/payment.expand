                            {{ lang('accept_expandPay') }}
                        </p>
                        <p class="payment-method-dec">
                            {{ lang('expandPay_fee') }}
                        </p>

                        <p class="payment-method-dec bold">
                            {{ lang('expandPay_transaction_fee') }}
                        </p>

                        <p class="payment-method-dec">
                            {{ lang('expandPay_method') }} 
                        </p>
                        <img src="view/image/payment/paymentmethods.jpg" class="payment-method-logos">
                        <div class="btn-holder">
                            <a href="{{ link('extension/expandpay/expandpaylogin') }}" class="btn btn-outline-primary ">{{ lang('activate_method') }} </a> 
                        </div>
                    </div>
                </div>
                <div class="col-md-6 col-sm-12">
                    <div class="payment-method-holder">
                        <img src="view/image/payment/paypal.jpg" class="payment-method-img">
                        <p class="payment-method-title">
                            {{ lang('accept_payPal') }}
                        </p>
                        <p class="payment-method-dec">
                            {{ lang('payPal_decs') }}
                        </p>
						<div class="section__description payment-method-dec">
                        <p> {{lang('paypal_shortDesc')}} </p>
                        <p>
                            <span class="learn-more"> {{lang('paypal_learnMore')}} </span>
                            <a class="paypal__descition-link" target="_blank" href="{{ paypal_supportLink }}">{{lang('paypal_titleName')}} <i class="fas fa-external-link-alt"></i></a>
                        </p>
                        </div>
                        <div class="btn-holder">
							{% if paypal_installed  %}
                                <a href="{{ link('extension/payment/activate?code=paypal&activated=1&payment_company=1') }}" class="btn btn-outline-primary">{{lang("activate_payPal")}}</a>
                            {% else %}
                                <a href="{{ link('extension/payment/install?extension=paypal&payment_company=1') }}" class="btn btn-outline-primary">{{lang("activate_payPal")}}</a>
                                    {% endif %}
                        </div>
                    </div>
                </div>
            </div>

