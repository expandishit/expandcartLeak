{% extends "base.expand" %}
{% from "controls/breadcrumb.expand" import breadcrumb as breadcrumb %}
{% import "controls/forms.expand" as forms %}

{% block secondarymenu %}
    {% include 'setting/settingmenu.expand' with {'expand': '#store', 'active': '#payment'} %}
{% endblock secondarymenu %}

{% set bodyClasses = "has-detached-right" %}


{% block title %}
    {{ lang('payments_providers') }}
{% endblock title %}

{% block breadcrumb %}
    {{ breadcrumb(breadcrumbs) }}
{% endblock breadcrumb %}

{% block headelements %}
{% endblock headelements %}

{% block content %}
    <style>
        .content-wrapper {
            /* background-color: #fff; */
        }
        /* start bread cramp */
        .page-header--paypal{
            margin: 0 20px;
            background-color: unset;
        }
         .page-header-sm .page-title{
            padding-right: 8px;
            padding-left: 8px;
        }
        .page-header--paypal .breadcrumb{
            display: none;
        }
         .page-header--paypal .page-header-content + .breadcrumb-line{
            margin: 0 32px;
        }
         .page-header--paypal .page-title h5{
            display: flex;
            align-items: center;
        }
         .page-header--paypal .page-title h5 span{
            margin: 0 10px;
        }
        /* end breadcramp */
        .page-title {
            max-width: 1200px;
            margin-right: auto;
            margin-left: auto;
        }
         .paypal-box{
            padding: 30px 25px;
            box-shadow: 0 0 5px 0 rgba(167, 162, 194, 0.34)
        }
         .paypal-box img{
            margin-bottom: 25px;
            margin-top: 5px;
        }
        .info-content{
            color: #4c4c51;
            font-size: 13px;
            font-weight: 500;
            line-height: 1.69;
            margin-bottom: 37px !important;
        }
         .paypal-box .box-btn{
            border-radius: 4px;
            background-color: #e0f5fe;
            color: #155ebc;
            font-size: 16px;
            font-weight: 600;
            border: none;
            padding: 11px 20px;
        }
         .section__description{
            color: #4c4c51;
            font-size: 13px;
            font-weight: 500;
        }
         .learn-more{
            font-weight: 600;
        }
         .learn-more > i{
            color: #155ebc;
        }
         .paypal__descition-link{
            font-weight: 600;
            color: #155ebc;
        }
         .list-wrapper{
            margin-top: 40px;
        }
         .content-container{
            border-bottom: solid 1px #e0ddef;
            margin-bottom: 30px;
        }
    </style>
   
  {% if show_expandpay %}
   <div class="list-wrapper">
        <div class="content-inner">
            {% if payment_enabled != 1 %}
            <p class="first-ship-alert">
                <i class="fa fa-exclamation" aria-hidden="true"></i>
                <span class="w-100">{{lang('text_first_shipping_alert')}}</span>
                <i class="fa fa-times" aria-hidden="true"></i>
            </p>
            {% endif %}

            {% include 'includes/tutorial_modal_snippet.expand' with {
