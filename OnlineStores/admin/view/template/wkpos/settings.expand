{% extends "base.expand" %}
{% from "controls/breadcrumb.expand" import breadcrumb as breadcrumb %}
{% import "controls/forms.expand" as forms %}

{% block title %}
    {{ lang('heading_title') }}
{% endblock title %}

{% block breadcrumb %}
    {{ breadcrumb(breadcrumbs) }}
{% endblock breadcrumb %}

{% block headelements %}
<span class="headLinksEffect">
    <a href="{{ cancel }}" data-toggle="tooltip" title="{{ button_cancel }}" class="btn btn-default">{{ button_cancel }}</a> 
    <button type="button" onclick="$('.form').submit();" data-style="zoom-in" class="btn btn-primary btn-ladda btn-ladda-spinner top-save-button"><i class="fa fa-save"></i> Save</button>

</span>
{% endblock headelements %}


{% block content %}
<div class="alert alert-info alert-styled-left alert-bordered" id="wkpos_init_alert" style="display: none">
    <button type="button" class="close" data-dismiss="alert"><span>Ã—</span><span class="sr-only">Close</span></button>
    {{ init_alert }}
</div>
<div class="panel panel-default">
    <ul class="nav nav-tabs nav-tabs-highlight nav-justified nav-tabs-top top-divided" style="margin-bottom: 0">
        <li class="active"><a data-toggle="tab" href="#tab-general">{{ tab_general }}</a></li>
        <li><a data-toggle="tab" href="#tab-customer">{{ tab_customer }}</a></li>
        <li><a data-toggle="tab" href="#tab-payment">{{ tab_payment }}</a></li>
        <li><a data-toggle="tab" href="#tab-shipping">{{ tab_shipping }}</a></li>
        <li><a data-toggle="tab" href="#tab-receipt">{{ tab_receipt }}</a></li>
        <li><a data-toggle="tab" href="#tab-barcode">{{ tab_barcode }}</a></li>
    </ul>
</div>
<form action="{{ action }}" method="post" enctype="multipart/form-data" id="form-wkpos" class="form">
  <input type='hidden' name='wkpos_init' value='1'/>
  <div class="tab-content">
    <!-- General TAB -->
    <div class="tab-pane active" id="tab-general">
      <div class="col-md-3">
        <div class="panel panel-white">
          <div class="panel-heading">
                  <h6 class="panel-title"><span class="text-bold">{{ text_status }}</span><a class="heading-elements-toggle"><i class="icon-more"></i></a></h6>
              <div class="heading-elements not-collapsible">
                  <ul class="icons-list">
                      <li><a data-action="collapse" class=""></a></li>
                  </ul>
              </div>
          </div>
          <div class="panel-body">
            <div class="form-group col-md-12">
                <label class="control-label text-semibold" for="input-status">{{ entry_status }}</label>
                <div class="checkbox checkbox-switchery no-margin">
                  <label>
                        <input type="checkbox" onchange="changeStatus(this);" name="wkpos_status" class="switchery" {{ wkpos_status == '1' or not wkpos_init  ? 'checked' }}>
                        <span class="switchery-status">
                          {% if wkpos_status == '1' or not wkpos_init  %} {{ text_enabled }} {% else %} {{ text_disabled }} {% endif %}
                        </span>
                  </label>
                </div>
              </div>
              <!--<div class="form-group col-md-12">
                <label class="control-label text-semibold" for="input-status">{{ entry_show_whole }}</label>
                <div class="checkbox checkbox-switchery no-margin">
                  <label>
                        <input type="checkbox" onchange="changeStatus2(this);" name="wkpos_show_whole" class="switchery" {{ wkpos_show_whole == '1'  or not wkpos_init  ? 'checked' }}>
                        <span class="switchery-status">
                          {% if wkpos_show_whole == '1' or not wkpos_init %} {{ text_yes }} {% else %} {{ text_no }} {% endif %}
                        </span>
                  </label>
                </div>
              </div>-->

              
              <div class="form-group col-md-12">
                <label class="control-label text-semibold" for="input-status">{{ entry_show_lowstock_prod }}</label>
                <div class="checkbox checkbox-switchery no-margin">
                  <label>
                        <input type="checkbox" onchange="changeStatus2(this);" name="wkpos_show_lowstock_prod" class="switchery" {{ wkpos_show_lowstock_prod == '1'  or not wkpos_init  ? 'checked' }}>
                        <span class="switchery-status">
                          {% if wkpos_show_lowstock_prod == '1' or not wkpos_init  %} {{ text_yes }} {% else %} {{ text_no }} {% endif %}
                        </span>
                  </label>
                </div>
              </div>
              <div class="form-group  col-md-12">
                <label class="control-label text-semibold" for="input-status">{{ entry_email_agent }}</label>
                <div class="checkbox checkbox-switchery no-margin">
                  <label>
                        <input type="checkbox" onchange="changeStatus2(this);" name="wkpos_email_agent" class="switchery" {{ wkpos_email_agent == '1'   ? 'checked' }}>
                        <span class="switchery-status">
                          {% if wkpos_email_agent == '1' %} {{ text_yes }} {% else %} {{ text_no }} {% endif %}
                        </span>
                  </label>
                </div>
              </div>
          </div>
        </div>
      </div>
      <div class="col-md-9">
        <div class="panel panel-white">
          <div class="panel-heading">
                  <h6 class="panel-title"><span class="text-bold">{{ text_settings }}</span><a class="heading-elements-toggle"><i class="icon-more"></i></a></h6>
              <div class="heading-elements not-collapsible">
                  <ul class="icons-list">
                      <li><a data-action="collapse" class=""></a></li>
                  </ul>
              </div>
          </div>
          <div class="panel-body">
            <div class="row">
              <div class="form-group col-md-6">
                <label class="control-label text-semibold" for="input-store-country">{{ entry_store_country }}</label>
                  <select name="wkpos_store_country_id" id="input-store-country" class="form-control select">
                    {# <option value="">{{ text_select }}</option>#}
                    {% for country in countries %}
                      {% if country['country_id'] == wkpos_store_country_id %}
                      <option value="{{ country['country_id'] }}" selected="selected">{{ country['name'] }}</option>
                      {% else %}
                      <option value="{{ country['country_id'] }}">{{ country['name'] }}</option>
                      {% endif %}
                    {% endfor %}
                  </select>
              </div>
              <div class="form-group col-md-6">
                <label class="control-label text-semibold" for="input-store-zone">{{ entry_store_zone }}</label>
                  <select name="wkpos_store_zone_id" id="input-store-zone" class="form-control select">
                  </select>
              </div>
              {#<div class="form-group col-md-12">
                <label class="control-label text-semibold" for="input-heading1"><span data-toggle="tooltip" title="{{ help_heading1 }}">{{ entry_heading1 }}</span></label>
                  <input type="text" name="wkpos_heading1" value="{{ wkpos_heading1 }}" placeholder="{{ entry_heading1 }}" id="input-heading1" class="form-control" />
              </div>
              <div class="form-group col-md-12">
                <label class="control-label text-semibold" for="input-heading2"><span data-toggle="tooltip" title="{{ help_heading2 }}">{{ entry_heading2 }}</span></label>
                  <input type="text" name="wkpos_heading2" value="{{ wkpos_heading2 }}" placeholder="{{ entry_heading2 }}" id="input-heading2" class="form-control" />
              </div>
              <div class="form-group col-md-12">
                <label class="control-label text-semibold" for="input-logcontent"><span data-toggle="tooltip" title="{{ help_logcontent }}">{{ entry_logcontent }}</span></label>
                  <textarea name="wkpos_logcontent" placeholder="{{ entry_logcontent }}" id="input-logcontent" class="form-control" rows="6">{{ wkpos_logcontent }}</textarea>
              </div>#}
              <div class="form-group col-md-6">
                <label class="control-label text-semibold" for="input-populars"><span data-toggle="tooltip" title="{{ help_populars }}">{{ entry_populars }}</span></label>
                  <input type="number" name="wkpos_populars" value="{{ wkpos_populars }}" min="0" placeholder="{{ entry_populars }}" id="input-populars" class="form-control" onkeypress="return validate(event, this);"/>
              </div>
              <div class="form-group col-md-6">
                <label class="control-label text-semibold" for="input-low-stock"><span data-toggle="tooltip" title="{{ help_low_stock }}">{{ entry_low_stock }}</span></label>
                  <input type="number" name="wkpos_low_stock" min="0" value="{{ wkpos_low_stock }}" placeholder="{{ entry_low_stock }}" id="input-low-stock" class="form-control" onkeypress="return validate(event, this);"/>
              </div>
              {% if price_list_status %}
              <div class="form-group col-md-6">
                <label class="control-label text-semibold" for="input-price-list"><span data-toggle="tooltip" title="{{ help_price_list }}">{{ entry_price_list_status }}</span></label>
                  <select class="form-control select" name="wkpos_price_list_status">
                    <option {% if wkpos_price_list_status %}selected{% endif %} value="1">{{ text_enabled }}</option>
                    <option {% if wkpos_price_list_status == 0 %}selected{% endif %} value="0">{{ text_disabled }}</option>
                  </select>
              </div>
              {% endif %}
              <div class="form-group col-md-12">
                <label class="control-label text-semibold" for="input-store-detail"><span data-toggle="tooltip" title="{{ help_store_detail }}">{{ entry_store_detail }}</span></label>
                  <textarea name="wkpos_store_detail" id="input-store-detail" class="form-control" rows="3">{{ wkpos_store_detail }}</textarea>
              </div>
            </div>
          </div>
        </div>
      </div>
    </div>
    <!-- /General TAB -->

    <!-- Customer TAB -->
    <div class="tab-pane" id="tab-customer">
      <div class="col-md-3">
        <div class="panel panel-white">
          <div class="panel-heading">
                  <h6 class="panel-title"><span class="text-bold">{{ text_status }}</span><a class="heading-elements-toggle"><i class="icon-more"></i></a></h6>
              <div class="heading-elements not-collapsible">
                  <ul class="icons-list">
                      <li><a data-action="collapse" class=""></a></li>
                  </ul>
              </div>
          </div>
          <div class="panel-body">
            <div class="form-group col-md-2">
              <label class="control-label text-semibold" for="input-status">{{ entry_newsletter }}</label>
              <div class="checkbox checkbox-switchery no-margin">
                <label>
                      <input type="checkbox" onchange="changeStatus(this);" name="wkpos_newsletter" class="switchery" {{ wkpos_newsletter == '1'   ? 'checked' }}>
                      <span class="switchery-status">
                        {% if wkpos_newsletter == '1' %} {{ text_enabled }} {% else %} {{ text_disabled }} {% endif %}
                      </span>
                </label>
              </div>
            </div>
          </div>
        </div>
      </div>
      <div class="col-md-9">
          <div class="panel panel-white">
            <div class="panel-heading">
                    <h6 class="panel-title"><span class="text-bold">{{ text_settings }}</span><a class="heading-elements-toggle"><i class="icon-more"></i></a></h6>
                <div class="heading-elements not-collapsible">
                    <ul class="icons-list">
                        <li><a data-action="collapse" class=""></a></li>
                    </ul>
                </div>
            </div>
            <div class="panel-body">
              <legend>{{ text_new_customer }}</legend>
                
                <div class="form-group col-md-6">
                  <label class="control-label text-semibold" for="input-new-customer-group"><span data-toggle="tooltip" title="{{ help_new_group }}">{{ entry_customer_group }}</label>
                    <select name="wkpos_new_customer_group_id" id="input-new-customer-group" class="form-control select">
                      {% for customer_group in customer_groups %}
                        {% if customer_group['customer_group_id'] == wkpos_new_customer_group_id %}
                      <option value="{{ customer_group['customer_group_id'] }}" selected="selected">{{ customer_group['name'] }}</option>
                      {% else %}
                      <option value="{{ customer_group['customer_group_id'] }}">{{ customer_group['name'] }}</option>
                      {% endif %}
                      {% endfor %}
                    </select>
                </div>
                

                <div class="form-group  col-md-6">
                  <label class="control-label text-semibold" for="input-password"><span data-toggle="tooltip" title="{{ help_password }}">{{ entry_password }}</span></label>
                    <input type="text" name="wkpos_customer_password" value="{{ wkpos_customer_password }}" placeholder="{{ entry_password }}" id="input-password" class="form-control" />
                </div>

                <legend>{{ text_default_customer }}</legend>
                <div class="form-group col-sm-6">
                  <label class="control-label text-semibold" for="input-customer-group">{{ entry_customer_group }}</label>
                    <select name="wkpos_customer_group_id" id="input-customer-group" class="form-control select">
                      {% for customer_group in customer_groups %}
                        {% if customer_group['customer_group_id'] == wkpos_new_customer_group_id %}
                      <option value="{{ customer_group['customer_group_id'] }}" selected="selected">{{ customer_group['name'] }}</option>
                      {% else %}
                      <option value="{{ customer_group['customer_group_id'] }}">{{ customer_group['name'] }}</option>
                      {% endif %}
                      {% endfor %}
                    </select>
                </div>
                <div class="form-group col-sm-6">
                  <label class="control-label text-semibold" for="input-firstname">{{ entry_firstname }}</label>
                    <input type="text" name="wkpos_firstname" value="{{ wkpos_firstname ? wkpos_firstname : (not wkpos_init ? accountuser['fname'] : '') }}" placeholder="{{ entry_firstname }}" id="input-firstname" class="form-control" />
                </div>
                <div class="form-group col-sm-6">
                  <label class="control-label text-semibold" for="input-lastname">{{ entry_lastname }}</label>
                    <input type="text" name="wkpos_lastname" value="{{ wkpos_lastname ? wkpos_lastname : (not wkpos_init ? accountuser['lname'] : '') }}" placeholder="{{ entry_lastname }}" id="input-lastname" class="form-control" />
                </div>
                <div class="form-group col-sm-6">
                  <label class="control-label text-semibold" for="input-email">{{ entry_email }}</label>
                    <input type="text" name="wkpos_email" value="{{ wkpos_email ? wkpos_email : (not wkpos_init ? accountuser['email'] : '') }}" placeholder="{{ entry_email }}" id="input-email" class="form-control" />
                </div>
                <div class="form-group col-sm-6">
                  <label class="control-label text-semibold" for="input-telephone">{{ entry_telephone }}</label>
                    <input type="text" name="wkpos_telephone" value="{{ wkpos_telephone ? wkpos_telephone : '00000' }}" placeholder="{{ entry_telephone }}" id="input-telephone" class="form-control" />
                </div>
                <div class="form-group col-sm-6">
                  <label class="control-label text-semibold" for="input-fax">{{ entry_fax }}</label>
                    <input type="text" name="wkpos_fax" value="{{ wkpos_fax }}" placeholder="{{ entry_fax }}" id="input-fax" class="form-control" />
                </div>
                <legend>{{ text_default_address }}</legend>
                <div class="form-group col-sm-6">
                  <label class="control-label text-semibold" for="input-company">{{ entry_company }}</label>
                    <input type="text" name="wkpos_company" value="{{ wkpos_company }}" placeholder="{{ entry_company }}" id="input-company" class="form-control" />
                </div>
                <div class="form-group col-sm-6">
                  <label class="control-label text-semibold" for="input-address-1">{{ entry_address_1 }}</label>
                    <input type="text" name="wkpos_address_1" value="{{ wkpos_address_1 ? wkpos_address_1 : '----' }}" placeholder="{{ entry_address_1 }}" id="input-address-1" class="form-control" />
                </div>
                <div class="form-group col-sm-6">
                  <label class="control-label text-semibold" for="input-address-2">{{ entry_address_2 }}</label>
                    <input type="text" name="wkpos_address_2" value="{{ wkpos_address_2 }}" placeholder="{{ entry_address_2 }}" id="input-address-2" class="form-control" />
                </div>
                <div class="form-group col-sm-6">
                  <label class="control-label text-semibold" for="input-city">{{ entry_city }}</label>
                    <input type="text" name="wkpos_city" value="{{ wkpos_city ? wkpos_city : '----'  }}" placeholder="{{ entry_city }}" id="input-city" class="form-control" />
                </div>
                <div class="form-group col-sm-6">
                  <label class="control-label text-semibold" for="input-postcode">{{ entry_postcode }}</label>
                    <input type="text" name="wkpos_postcode" value="{{ wkpos_postcode }}" placeholder="{{ entry_postcode }}" id="input-postcode" class="form-control" />
                </div>
                <div class="form-group col-sm-6">
                  <label class="control-label text-semibold" for="input-country">{{ entry_country }}</label>
                    <select name="wkpos_country_id" id="input-country" class="form-control select">
                      {#<option value="">{{ text_select }}</option>#}
                      {% for country in countries %}
                        {% if country['country_id'] == wkpos_country_id %}
                      <option value="{{ country['country_id'] }}" selected="selected">{{ country['name'] }}</option>
                      {% else %}
                      <option value="{{ country['country_id'] }}">{{ country['name'] }}</option>
                       {% endif %}
                      {% endfor %}
                    </select>
                </div>
                <div class="form-group col-sm-6">
                  <label class="control-label text-semibold" for="input-zone">{{ entry_zone }}</label>
                    <select name="wkpos_zone_id" id="input-zone" class="form-control select">
                    </select>
                </div>
            </div>
          </div>
        </div>
    </div>
    <!-- /Customer TAB -->

    <!-- Payment TAB -->
    <div class="tab-pane" id="tab-payment">
      <div class="col-md-3">
        <div class="panel panel-white">
          <div class="panel-heading">
                  <h6 class="panel-title"><span class="text-bold">{{ text_status }}</span><a class="heading-elements-toggle"><i class="icon-more"></i></a></h6>
              <div class="heading-elements not-collapsible">
                  <ul class="icons-list">
