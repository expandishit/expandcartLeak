            </div>
            <div class="form-group col-md-12">
              <label class="control-label text-semibold" for="input-status">{{ entry_credit_status }}</label>
              <div class="checkbox checkbox-switchery no-margin">
                <label>
                      <input type="checkbox" onchange="changeStatus(this);" name="wkpos_credit_status" class="switchery" {{ wkpos_credit_status == '1'   ? 'checked' }}>
                      <span class="switchery-status">
                        {% if wkpos_credit_status == '1' %} {{ text_enabled }} {% else %} {{ text_disabled }} {% endif %}
                      </span>
                </label>
              </div>
            </div>
            <div class="form-group col-md-12">
              <label class="control-label text-semibold" for="input-status">{{ entry_discount_status }}</label>
              <div class="checkbox checkbox-switchery no-margin">
                <label>
                      <input type="checkbox" onchange="changeStatus(this);" name="wkpos_discount_status" class="switchery" {{ wkpos_discount_status == '1'  or not wkpos_init ? 'checked' }}>
                      <span class="switchery-status">
                        {% if wkpos_discount_status == '1' or not wkpos_init %} {{ text_enabled }} {% else %} {{ text_disabled }} {% endif %}
                      </span>
                </label>
              </div>
            </div>
            <div class="form-group col-md-12">
              <label class="control-label text-semibold" for="input-status">{{ entry_coupon_status }}</label>
              <div class="checkbox checkbox-switchery no-margin">
                <label>
                      <input type="checkbox" onchange="changeStatus(this);" name="wkpos_coupon_status" class="switchery" {{ wkpos_coupon_status == '1' or not wkpos_init  ? 'checked' }}>
                      <span class="switchery-status">
                        {% if wkpos_coupon_status == '1'  or not wkpos_init %} {{ text_enabled }} {% else %} {{ text_disabled }} {% endif %}
                      </span>
                </label>
              </div>
            </div>
            <div class="form-group col-md-12">
              <label class="control-label text-semibold" for="input-status">{{ entry_tax_status }}</label>
              <div class="checkbox checkbox-switchery no-margin">
                <label>
                      <input type="checkbox" onchange="changeStatus(this);" name="wkpos_tax_status" class="switchery" {{ wkpos_tax_status == '1'  or not wkpos_init ? 'checked' }}>
                      <span class="switchery-status">
                        {% if wkpos_tax_status == '1' or not wkpos_init %} {{ text_enabled }} {% else %} {{ text_disabled }} {% endif %}
                      </span>
                </label>
              </div>
            </div>
          </div>
        </div>
      </div>
      <div class="col-md-9">
        <div class="panel panel-white">
          <div class="panel-heading">
                  <h6 class="panel-title"><span class="text-bold">{{ text_settings }}</span><a class="heading-elements-toggle"><i class="icon-more"></i></a></h6>
              <div class="heading-elements not-collapsible">
                  <ul class="icons-list">
                      <li><a data-action="collapse" class=""></a></li>
                  </ul>
              </div>
          </div>
          <div class="panel-body">
            <div class="payment-tap">
                  <div class="form-group col-md-8">
                     <div class="tabbable nav-tabs-vertical nav-tabs-right">
                          <div class="tab-content">
                              {% for language in languages %}
                                  <div class="tab-pane has-padding {{ loop.first ? 'active' : '' }}" id="langTab1{{ language['code'] }}">
                                      <div class="form-group" id="cash-group[{{ language['code'] }}]">
                                          <label class="control-label text-semibold">{{ lang('entry_cash_title') }}<span class="text-danger">*</span></label>
                                          <input type="text" class="form-control" id="wkpos_cash_title{{ language['language_id'] }}" name="wkpos_cash_title{{ language['language_id'] }}" value="{{ wkpos_cash_title[language['language_id']] ? wkpos_cash_title[language['language_id']] : 'Cash' }}" placeholder="{{ entry_cash_title }}">
                                          <span class="help-block"></span>
                                          <span class="text-muted">{{ langempty('cash_help') }}</span>
                                      </div>
                                  </div>
                              {% endfor %}
                          </div>
                          <ul class="nav nav-tabs nav-tabs-highlight nav-tabs-lang">
                              {% for language in languages %}
                                  <li class="{{ loop.first ? 'active' : '' }}">
                                      <a href="#langTab1{{ language['code'] }}" data-toggle="tab" aria-expanded="false">
                                          <img src="view/image/flags/{{ language['image'] }}" title="{{ language['name'] }}" class="pull-right">
                                          <div> {{ language['name'] }}</div>
                                      </a>
                                  </li>
                              {% endfor %}
                          </ul>
                      </div>
                  </div>
                  <div class="form-group col-md-4">
                      <label class="control-label text-semibold" for="input-cash-order-status">
                          <span data-toggle="tooltip" title="{{ help_cash_status }}">{{ entry_cash_order_status }}</span></label>
                      <select name="wkpos_cash_order_status_id" id="input-cash-order-status" class="form-control select">
                          {% for order_status in order_statuses %}
                              {% if order_status['order_status_id'] == wkpos_cash_order_status_id %}
                                  <option value="{{ order_status['order_status_id'] }}" selected="selected">{{ order_status['name'] }}</option>
                              {% else %}
                                  <option value="{{ order_status['order_status_id'] }}">{{ order_status['name'] }}</option>
                              {% endif %}
                          {% endfor %}
                      </select>
                  </div>
                  <div style="clear: both;"></div>
                  <div class="form-group col-md-8">
                      <div class="tabbable nav-tabs-vertical nav-tabs-right">
