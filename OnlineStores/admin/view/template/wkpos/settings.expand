              <label class="control-label text-semibold" for="select-barcode-generate"><span data-toggle="tooltip" title="{{ help_generate_with }}">{{ entry_generate_with }}</span></label>
                <select class="form-control select" name="wkpos_generate_barcode" id="select-barcode-generate">
                  <option {% if wkpos_generate_barcode == 'barcode' %} selected {% endif %} value="barcode">{{ text_barcode }}</option>
                  <option {% if wkpos_generate_barcode == 'id' %} selected {% endif %} value="id">{{ text_id }}</option>
                  <option {% if wkpos_generate_barcode == 'upc'  %} selected {% endif %} value="upc">{{ text_upc }}</option>
                  <option {% if wkpos_generate_barcode == 'sku' %} selected {% endif %} value="sku">{{ text_sku }}</option>
                  <option {% if wkpos_generate_barcode == 'ean' %} selected {% endif %} value="ean">{{ text_ean }}</option>
                </select>
            </div>
            <div class="form-group col-md-4">
              <label class="control-label text-semibold" for="input-slot-number"><span data-toggle="tooltip" title="{{ help_number_slot }}">{{ entry_number_slot }}</span></label>
                <input type="number" id="input-slot-number" name="wkpos_barcode_slot" value="{{ wkpos_barcode_slot ? wkpos_barcode_slot : 1 }}" class="form-control"  min="5" placeholder="{{ entry_number_slot }}" onkeypress="return validate(event, this, true)"/>
            </div>
            <div class="form-group col-md-12">
              <label class="control-label text-semibold" for="input-barcode-image">{{ entry_barcode_type }}</label>
            </div>
            {% if wkpos_barcode_type == 'vertical' %}
            <div>
              <input type="radio" name="wkpos_barcode_type" value="horizontal">{{ text_horizontal }}<img src="../image/wkpos/horizontal.png"/>
            </div>
            <div>
              <input type="radio" name="wkpos_barcode_type" value="vertical" checked="checked" >{{ text_vertical }}<img style="margin-left: 31px;" src="../image/wkpos/vertical.png"/>
            </div>
            {% else %}
            <div>
             <input type="radio" name="wkpos_barcode_type" value="horizontal"checked="checked" >{{ text_horizontal }} <img src="../image/wkpos/horizontal.png"/>
            </div>
            <div>
              <input type="radio" name="wkpos_barcode_type" value="vertical">{{ text_vertical }}<img style="margin-left: 31px;" src="../image/wkpos/vertical.png"/>
            </div>
            {% endif %}
            
          </div>
        </div>
      </div>
    </div>
    <!-- /Barcode TAB -->
  </div>
</form>
<script type="text/javascript"><!--

{% if not wkpos_init %}
