              <div class="checkbox checkbox-switchery no-margin">
                <label>
                      <input type="checkbox" onchange="changeStatus(this);" name="wkpos_cash_status" class="switchery" {{ wkpos_cash_status == '1' or not wkpos_init  ? 'checked' }}>
